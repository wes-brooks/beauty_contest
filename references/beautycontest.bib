@article{Wade:2008yi,
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/wade_2008_high_sensitivity_children_gastrointestinal_illness.pdf},
	year = {2008},
	date = {2010-12-23 11:37:42 -0600},
	address = {United States},
	number = {3},
	pages = {375--383},
	isbn = {1044-3983},
	title = {High sensitivity of children to swimming-associated gastrointestinal illness: results using a rapid assay of recreational water quality},
	m = {Journal Article},
	journal = {Epidemiology (Cambridge, Mass.)},
	av = {Print},
	ty = {JOUR},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/wade_2008_high_sensitivity_children_gastrointestinal_illness.pdf},
	annote = {BACKGROUND: Culture-based methods of monitoring fecal pollution in recreational waters require 24 to 48 hours to obtain results. This delay leads to potentially inaccurate management decisions regarding beach safety. We evaluated the quantitative polymerase chain reaction (QPCR) as a faster method to assess recreational water quality and predict swimming-associated illnesses. METHODS: We enrolled visitors at 4 freshwater Great Lakes beaches, and contacted them 10 to 12 days later to ask about health symptoms experienced since the visit. Water at the beaches was polluted by point sources that carried treated sewage. We tested water samples daily for Enterococcus using QPCR and membrane filtration (EPA Method 1600). RESULTS: We completed 21,015 interviews and tested 1359 water samples. Enterococcus QPCR cell equivalents (CEs) were positively associated with swimming-associated gastrointestinal (GI) illness (adjusted odds ratio per 1 log10 QPCR CE =1.26; 95{\%} confidence interval = 1.06-1.51). The association between GI illness and QPCR CE was stronger among children aged 10 years and below (1.69; 1.24-2.30). Nonenteric illnesses were not consistently associated with Enterococcus QPCR CE exposure, although rash and earache occurred more frequently among swimmers. Enterococcus QPCR CE exposure was more strongly associated with GI illness than Enterococcus measured by membrane filtration. CONCLUSIONS: Measurement of the indicator bacteria Enterococci in recreational water using a rapid QPCR method predicted swimming-associated GI illness at freshwater beaches polluted by sewage discharge. Children at 10 years or younger were at greater risk for GI illness following exposure.},
	volume = {19},
	author = {Wade,Timothy J. and Calderon,Rebecca L. and Brenner,Kristen P. and Sams,Elizabeth and Beach,Michael and Haugland,Richard and Wymer,Larry and Dufour,Alfred P.},
	month = {May}
}

@article{Wade:2006qc,
	address = {U.S. EPA Human Studies Division, MD 58A, Research Triangle Park, NC 27711 USA, {$[$}mailto:Calderon.rebecca{\char64}epa.gov{$]$}},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/wade_2006_rapidly_measured_indicators_predictive_gastrointestinal_illness.pdf},
	year = {2006},
	month = {Jan},
	ty = {JOUR},
	date = {2010-12-23 11:37:44 -0600},
	keywords = {Article Subject Terms: Recreational waters; Prediction; Public health; Polymerase chain reaction; Environmental monitoring; Symptoms; Water quality; Recreation areas; Lakes; Gastrointestinal tract; Beaches; Water-borne diseases; Swimming; Assessments; Water Quality Standards; Indicators; Water Analysis; Testing Procedures; Water Sampling; Article Taxonomic Terms: Enterococcus; Bacteroides; Article Geographic Terms: North America, Erie L.; USA, Michigan L.; North America, Great Lakes; DNA; Freshwater},
	m = {Journal Article},
	number = {1},
	journal = {Environmental health perspectives},
	isbn = {0091-6765},
	pages = {24--28},
	title = {Rapidly Measured Indicators of Recreational Water Quality Are Predictive of Swimming-Associated Gastrointestinal Illness},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/wade_2006_rapidly_measured_indicators_predictive_gastrointestinal_illness.pdf},
	annote = {Standard methods to measure recreational water quality require at least 24 hr to obtain results, making it impossible to assess the quality of water within a single day. Methods to measure recreational water quality in less than or equal to 2 hr have been developed. Application of rapid methods could give considerably more accurate and timely assessments of recreational water quality. We conducted a prospective study of beachgoers at two Great Lakes beaches to examine the association between recreational water quality, obtained using rapid methods, and gastrointestinal (GI) illness after swimming. Beachgoers were asked about swimming and other beach activities and 10-12 days later were asked about the occurrence of GI symptoms. We tested water samples for Enterococcus and Bacteroides species using the quantitative polymerase chain reaction (PCR) method. We observed significant trends between increased GI illness and Enterococcus at the Lake Michigan beach and a positive trend for Enterococcus at the Lake Erie beach. The association remained significant for Enterococcus when the two beaches were combined. We observed a positive trend for Bacteroides at the Lake Erie beach, but no trend was observed at the Lake Michigan beach. Enterococcus samples collected at 0800 hr were predictive of GI illness that day. The association between Enterococcus and illness strengthened as time spent swimming in the water increased. This is the first study to show that water quality measured by rapid methods can predict swimming-associated health effects.},
	volume = {114},
	author = {Wade,T. J. and Calderon,R. L. and Sams,E. and Beach,M. and Brenner,K. P. and Williams,A. H. and Dufour,A. P.}
}

