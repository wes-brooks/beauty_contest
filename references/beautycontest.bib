	address = {Affiliation: Environmental Water Studies, Department of Civil and Environmental Engineering, Stanford University, Stanford, CA 94305-4020, United States; Correspondence Address: Boehm, A. B.; Environmental Water Studies; Department of Civil and (TRUNCATED)},
	title = {Submarine discharge of nutrient-enriched fresh groundwater at Stinson Beach, California is enhanced during neap tides},
	keywords = {concentration (composition); discharge; groundwater; groundwater flow; mass balance; nutrient enrichment; residential location; saline intrusion; California; North America; Stinson Beach; United States},
	number = {4},
