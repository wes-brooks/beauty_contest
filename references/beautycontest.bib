@ARTICLE{Geldreich:1975oz,
  author = {Geldreich,E. E.},
  title = {Microbiological criteria concepts for coastal bathing waters},
  journal = {Ocean Management},
  year = {1975},
  volume = {2},
  pages = {225--248},
  number = {3},
  annote = {Criteria for water quality must be based on parameters that relate
	primarily to health hazards. Other considerations may include parameters
	that relate to aesthetic enjoyment of the beach area, fishing and
	shellfish culture. Coastal waters receiving discharges of sewage,
	stormwater and other animal wastes may contain disease causing bacteria,
	viruses, and multicellular parasites. Unfortunately, there is no
	single test that will adequately detect and quantitate the wide spectrum
	of pathogens that might be present. For this reason, major emphasis
	has been placed on fecal coliform or fecal coli measurements as evidence
	of contamination from warm-blooded animal waste discharges. Therefore,
	it is assumed that the potential for enteric disease increases as
	fecal coliform desities increase. The development of fecal coliform
	to fecal streptococcus ratios may be used to identify fecal pollution
	either as domestic sewage discharges or as nonhuman wastes transported
	via stormwater runoff. Pseudomonas aeruginosa enumeration may be
	of importance in warm estuarine environments subject to slow flushing
	rates and nutrient discharges. Although there is still insufficient
	specific epidemiological data available to correlate microbiological
	standards with the general quality of recreational waters, practical
	limits must be established to prevent further water quality degradation,
	down-grading of aesthetic quality, and increased public-health risk.
	The fecal coliform or fecal coli standards used in various countries
	have been based either on a geometric mean of 200 organisms per 100
	ml, or an upper limit of 1,000 organisms per 100 ml, which should
	never be exceeded or, if reached, reached only with a restricted,
	specified frequency. Statistical analysis of these two standard approaches
	indicates that the established limitations are essentially the same.
	Meaningful application of water-quality standards must be based on
	the collection of adequate bacteriological data and sanitary-survey
	intelligence. {\copyright}1974.},
  annote1 = {Cited By (since 1996): 5},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/geldreich_1975_microbiological_criteria_concepts_coastal_bathing_waters.pdf},
