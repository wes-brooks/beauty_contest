@ARTICLE{Cabelli:1976eo,
  author = {Cabelli,V. J. and Kennedy,H. and Levin,M. A.},
  title = {Pseudomonas aeruginosa fecal coliform relationships in estuarine
	and fresh recreational waters},
  journal = {J.WATER POLLUT.CONTROL FED.},
  year = {1976},
  volume = {48},
  pages = {367--376},
  number = {2},
  address = {Affiliation: Recreational Water Qual. Criteria Program, Nat. Mar.
	Water Quality Lab., US Environm. Protect. Agency, W. Kingston, R.I.,
	United States; Correspondence Address: Cabelli, Victor J.United States},
  annote = {Although, previously, ear and eye infections caused by P. aeruginosa
	constitute a major hazard of swimming, there is no conclusive evidence
	demonstrating a cause and effect relationship of P. aeruginosa densities
	in the water to the incidence of these infections in swimming populations.
	Because about 15 percent of the human population have the organism
	in the ear as part of the ''normal'' flora, it is entirely possible
	that it is these individuals who actually are at risk, that the source
	of the organisms is endogenous, and that ''trauma'' to the ear canals
	associated with swimming (especially underwater swimming) and diving
	allows these organisms to invade the tissues. In fact, individuals
	who normally carry this organism may not be ''normal'' at all in
	the sense that they may already have lost one or more of the barriers
	that prevent the growth of P. aeruginosa in the ear canal. These
	individuals will have to be identified in the epidemiological studies
	required to determine the need for criteria and standards governing
	P. aeruginosa densities in recreational waters. The data presented
	herein suggest two situations for this type of epidemiological study.
	The observation that, at the Lake Michigan beaches, fecal pollution
	was carried in by north winds and that P. aeruginosa densities peaked
	1 to 2 days following a north wind would suggest these or other beaches
	subject to fluctuating pollution for such a study. Secondly, high
	P. aeruginosa densities could be expected wherever recreational waters
	are subject to the discharge of nutrients.},
  annote1 = {Cited By (since 1996): 4},
  bdsk-url-1 = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0016924470&partnerID=40},
