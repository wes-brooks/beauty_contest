@ARTICLE{Toranzos:1990xo,
  author = {Toranzos,G. A. and Hernandez Delgado,E. A.},
  title = {Isolation of Bacteriophages from Large Volumes of Pristine Tropical
	Waters and Their Correlation to Fecal Contamination},
  journal = {Tropical Hydrology and Caribbean Water Resources.Proceedings of the
	International Symposium on Tropical Hydrology and Fourth Caribbean
	Islands Water Resources Congress, San Juan, Puerto Rico},
  year = {1990},
  volume = {Bethesda},
  month = {Maryland. (},
  address = {July 22-27, 1990. American Water Resources Association},
  address1 = {Puerto Rico Univ},
  annote = {Recently, strong evidence has been found against the use of coliforms
	as indicator organisms of fecal contamination in tropical waters.
	False positive and false negative results are commonly found. There
	is a need for the development of more rapid low-cost techniques for
	determining microbiological water quality. A method has been developed
	to analyze large volumes of pristine tropical waters for the presence
	of coliphages. It involves the filtration of water through positively
	charged filters and an elution step. The eluent is then mixed with
	nutrient agar and incubated. Water samples were analyzed from a rain
	forest, ranging from pristine river waters and water collected from
	bromeliads (epiphytic vegetation), to recreational rivers. Total
	and fecal coliforms as well as fecal streptococci levels detected
	were higher than recommended standards for fecal contamination of
