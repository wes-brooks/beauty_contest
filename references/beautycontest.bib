@article{Blanco:2004mu,
	author = {Blanco,H. A. S. and Nordelo,M. A. and Exp{\'o}sito,N. and Vargas,B.},
	m = {Journal Article},
	journal = {Rev.Fac.Ing.},
	ty = {JOUR},
	number = {1},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-4444303565&partnerID=40},
	year = {2004},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-4444303565&partnerID=40},
	date = {2010-12-23 11:37:42 -0600},
	pages = {21--27},
	title = {Coliforms in marine environment of protected areas with abundant birds, ¿pollution indicators? Study case: National Park Archipi{\'e}lago Los Roques},
	keywords = {Bacteriological Indicators; Bacteriological Quality in Protected Areas; Beaches; Birds; Coliforms; Coliform bacteria; Environmental impact; Environmental protection; Sewage; Solid wastes; Water bacteriology; Water quality; Pollution indicators; Marine pollution},
	isbn = {07984065},
	annote = {Tourist boom of the National Park Archipielago Los Roques in Venezuela has taken place in recent times and as consequence also the housing and boarding has increased, demanding public services such as energy, water and communications and producing larger amounts of solid wastes and wastewater. Isolated previous determinations of water quality in the beaches showed presence of faecal coliforms in concentrations above those admitted by Venezuelan standards for beach water (200 to 400 org/100 ml) being attributed their presence to the percolation of sewage from the septic tanks of the community (there is no sewerage). However, in the Park there are abundant birds and they could be the principal cause of the coliform presence. Based on these assumptions research was conducted to determine the influence of birds in the bacteriological quality of the beach water, and to evaluate it four distinctive sampling zones were chosen: a) only human presence; b) only birds presence; c) human and birds presence and d) no presence of humans or birds as blank. Coliform and other species were determined in every one of the zones and results showed that septic tanks were not the cause of a significant presence of Coliforms because temporary and seasonal variability was related to activity and presence of birds in the National Park. Escherichia coli, Klebsiella pneumonias and Enterobacter cloacae were the predominant species in the birds shelter zone as well as in the human presence, being their density and relationship with the physical surroundings the factors that allowed to define the origin of these pollution indicators. It is necessary to continue research in order to find a microbiological indicator that may discriminate the origin of these organisms in protected areas with abundance of birds and answer the question: ¿Are they indicators of human caused pollution?},
	volume = {19}
}

