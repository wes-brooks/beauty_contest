	ty = {JOUR},
	pages = {228--236},
	title = {Viability of Escherichia coli O157:H7 in natural river water determined by the use of flow cytometry},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/tanaka_2000_viability_ecoli_o157h7_natural_river_water_flow_cytometry.pdf},
	year = {2000},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/tanaka_2000_viability_ecoli_o157h7_natural_river_water_flow_cytometry.pdf},
	date = {2010-12-23 11:37:42 -0600}
}

