  keywords = {drinking water; environmental health; methodology; prevention; public
	health; recreation; water pollution},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0015991065&partnerID=40}
}

