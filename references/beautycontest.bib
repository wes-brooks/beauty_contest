@article{Carnus:1999lb,
	journal = {Tech. Sci. Methodes},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0032801336&partnerID=40&rel=R8.0.0},
	year = {1999},
	m = {Journal Article},
	annote = {Recent studies that have been carried out for the implementation of the SICAPG municipal sewage network, were a good illustration of analysis and measurement methods we use to follow in such cases. The public association for the said Cote d'Amour and Guerande Peninsula (SICAPG) gathers several communes, economically speaking, very sensitive to water quality cause of tourism and shellfish production activities. Effluents coming from these communes are currently treated and outfallen at sea in a unique point. First analysis consisted in making an inventory of coastal-related activities. Then, we analysed the problems in relation to water quality on this area, in the objective of drawing up a map emphasising sensitivity to urban sewage at sea. Afterwards, we undertook a series of measurements on site aiming at evaluating the influence of present wastewater flowed into the sea and get data for numerical modelling. Simultaneously, calculation of currents by mathematical model were conducted in different meteorological and oceanographic conditions, from the mouth of the very South part of the Loire river, to Pen-Be, close to the mouth of the North part of the Vilaine river These data were compared with measurements collected, in order to simulate outfalls using a mathematical model of water quality. Concentrations (bacterial and heavy metals) contained in the sea, were calculated in various wind and tide conditions, and in different position of outlets. Therefore, the impact of present wastewater effluents at sea were analysed and other geographic zones could have been tested. The results extracted from this study lead to different scenarios and produced an outstanding tool for municipal sewage system scheme decision making.},
	date = {2010-12-23 11:37:42 -0600},
	author = {Carnus,F. and Commaille,J. F. and Violleau,D.},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0032801336&partnerID=40&rel=R8.0.0},
	ty = {JOUR},
	pages = {55--73},
	title = {Sewerage in coastal area. An example: the municipal sewage system scheme of the SICAPG},
	isbn = {02997258},
	keywords = {effluent transport; mathematical modeling; sea outfall; sewage disposal; water quality; effluent; France},
	number = {3}
}

