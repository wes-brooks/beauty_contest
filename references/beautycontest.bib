@ARTICLE{Boehm:2002we,
  author = {Boehm,A. B. and Grant,S. B. and Kim,J. H. and Mowbray,S. L. and McGee,C.
	D. and Clark,C. D. and Foley,D. M. and Wellman,D. E.},
  title = {Decadal and shorter period variability of surf zone water quality
	at Huntington Beach, California},
  journal = {Environ.Sci.Technol.},
  year = {2002},
  volume = {36},
  pages = {3885--3892},
  number = {18},
  address = {Affiliation: Henry Samueli School of Engineering, Department of Chemical
	Engineering, University of California, Irvine, CA 92697, United States;
	Correspondence Address: Grant, S.B.; Henry Samueli School of Engineering;
	Department of Chemical Eng(TRUNCATED)},
  annote = {The concentration of fecal indicator bacteria in the surf zone at
	Huntington Beach, CA, varies over time scales that span at least
	7 orders of magnitude, from minutes to decades. Sources of this variability
	include historical changes in the treatment and disposal of wastewater
	and dry weather runoff, El Ni{\~n}o events, seasonal variations in
	rainfall, spring-neap tidal cycles, sunlight-induced mortality of
	bacteria, and nearshore mixing. On average, total coliform concentrations
	have decreased over the past 43 years, although point sources of
	shoreline contamination (storm drains, river outlets, and submarine
	outfalls) continue to cause transiently poor water quality. These
	transient point sources typically persist for 5-8 yr and are modulated
	by the phase of the moon, reflecting the influence of tides on the
	sourcing and transport of pollutants in the coastal ocean. Indicator
	bacteria are very sensitive to sunlight; therefore, the time of day
	when samples are collected can influence the outcome of water quality
	testing. These results demonstrate that coastal water quality is
	forced by a complex combination of local and external processes and
	raise questions about the efficacy of existing marine bathing water
	monitoring and reporting programs.},
