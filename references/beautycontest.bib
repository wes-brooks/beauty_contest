	ty = {CONF},
	address = {Affiliation: XCG Consultants Ltd., 2620 Bristol Circle, {\#}300, Oakville, Ont. L6H 6Z7, Canada; Affiliation: Department of Civil Engineering, University of Toronto, Toronto, Ont. M5S 1A4, Canada; Correspondence Address: Xu, L.; XCG Consultants Ltd(TRUNCATED)},
