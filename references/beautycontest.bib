}

@article{Harris:2004nd,
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-4444371695&partnerID=40&rel=R8.0.0},
	m = {Journal Article},
	pages = {61--70},
	number = {1},
	title = {Modelling hydroenvironmental and health risk assessment parameters along the South Wales Coast},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/harris_2004_modelling_hydroenvironmental_health_risk_wales_coast.pdf},
	year = {2004},
