	year = {1990},
	pages = {422--427},
	date = {2010-12-23 11:37:43 -0600},
	title = {New airborne sensors for monitoring estuaries and coastal waters in conjunction with satellites},
	keywords = {Oceanography - Coastal Zones; Remote Sensing - Multispectral Scanners; Water Resources - Estuarine; Airborne Sensors; Coastal Waters Monitoring; Estuary Monitoring; Satellite Sensing; Image Sensors},
	publisher = {Publ by IEEE},
