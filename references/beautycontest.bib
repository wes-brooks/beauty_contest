@article{Wiedenmann:2007os,
	number = {6},
	m = {Journal Article},
	isbn = {14308681},
	keywords = {Bathing water; Disease risk; Escherichia coli; Gastroenteritis; Intestinal enterococci; Water quality; Enterococcus},
	address = {Affiliation: Gesundheitsamt Esslingen a.N.; Affiliation: Landratsamt Esslingen, Gesundheitsamt, Sachgebiet 214: Infektionsschutz und Umwelthygiene, Beblingerstr. 2, 73728 Esslingen a.N., Germany; Correspondence Address: Wiedenmann, A.; Landratsa(TRUNCATED)},
	ty = {JOUR},
	pages = {351--365},
	title = {Health-related interpretation of individual microbiological sample results from monitoring programs of recreational waters based on recent epidemiological findings},
	annote = {Background: It would be inappropriate to use the percentile values for intestinal enterococci (IE) and Escherichia coli (EC) in Annex I of the new European bathing water directive 2006/7/EEC for the health-related classification of single sample results. Method: To provide guidance for a health-related interpretation of single sample results it was calculated which percentage of the bathers would probably be exposed above the epidemiologically verified thresholds of effect or No Observed Adverse Effect Levels (NOAELs) for gastroenteritis if a single sample result is interpreted as the median value of the distribution of microbial concentrations that is present in the bathing water on the sampling day. Results: If a representative sample from a bathing water reveals a concentration above ca. 250 IE/100 ml or above ca. 1,000 EC/ 100 ml, it may be assumed that on that sampling day >99{\%} of the bathers were exposed to concentrations above the epidemiologically verified NOAEL of 25 IE/1 100 ml or 10 EC/100 ml. For any other result below these concentrations the corresponding percentage can be read off a table or calculated using a simple formula. Conclusions: The calculation of the probability of being exposed to concentrations above a threshold of effect (NOAEL) transforms concentrations of indicator organisms, which in and of themselves cannot be interpreted, into meaningful figures which can easily be understood by the consumer. Thus, beach managers, supervising authorities, non governmental organisations, and political decision makers are provided with guidance for the interpretation of monitoring results, which may assist them in justifying intervention measures (beach warnings, beach closures etc.) and the repeal of such measures, and in initiating remedial actions if necessary. Action limits for health protection interventions should by no means be higher than 250 IE/100 ml or 1,000 EC/100 ml.},
	volume = {12},
	author = {Wiedenmann,A.},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-38149055749&partnerID=40},
	year = {2007},
	journal = {Umweltmed.Forsch.Prax.},
	date = {2010-12-23 11:37:42 -0600},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-38149055749&partnerID=40}
}

@article{Wiedenmann:2006cg,
	m = {Journal Article},
	keywords = {Article Subject Terms: Water quality; Intestine; Gastroenteritis; Pathogens; Freshwater environments; Risk assessment; Infectious diseases; swallowing; Consumers; Recreation areas; Water quality standards; Water-borne diseases; Environmental health; Water sampling; Compliance; Recreational waters; Bathing; Biological pollutants; Microbiological analysis; Public health; Diseases (gastro-intestinal); Diseases (Waterborne); Risk; Escherichia coli; Clostridium; Indicators; Microorganisms; Article Taxonomic Terms: Escherichia coli; Clostridium perfringens; Article Geographic Terms: Germany; Freshwater},
	month = {Feb},
	number = {2},
	address = {Landratsamt Loerrach, Fachbereich Gesundheit, Palmstr. 3, 79537 Loerrach, Germany, {$[$}mailto:albrecht.wiedenmann{\char64}arcor.de{$]$}},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/wiedenmann_2006_trial_assessing_disease_risks_concentration_ecoli_enterococci_clostridium_coliphages.pdf},
	year = {2006},
	date = {2010-12-23 11:37:44 -0600},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/wiedenmann_2006_trial_assessing_disease_risks_concentration_ecoli_enterococci_clostridium_coliphages.pdf},
	ty = {JOUR},
	pages = {228--236},
	annote = {We performed epidemiologic studies at public freshwater bathing sites in Germany to provide a better scientific basis for the definition of recreational water quality standards. A total of 2,196 participants were recruited from the local population and randomized into bathers and non-bathers. Bathers were exposed for 10 min and had to immerse their head at least three times. Water samples for microbiological analysis were collected at 20-min intervals. Unbiased concentration-response effects with no-observed-adverse-effect levels (NOAELs) were demonstrated for three different definitions of gastroenteritis and four fecal indicator organisms. Relative risks for bathing in waters with levels above NOAELs compared with nonbathing ranged from 1.8 (95{\%} CI, 1.2-2.6) to 4.6 (95{\%} CI, 2.1-10.1), depending on the definition of gastroenteritis. The effect of swallowing water provided additional evidence for true dose-response relationships. Based on the NOAELs, the following guide values for water quality are suggested: 100 Escherichia coli, 25 intestinal enterococci, 10 somatic coliphages, or 10 Clostridium perfringens per 100 mL. Recreational water quality standards are intended to protect the health of those consumers who are not already immune or resistant to pathogens that may be associated with indicator organisms. In contrast to current World Health Organization recommendations, we concluded that standards should be based on rates of compliance with NOAELs rather than on attributable risks determined above NOAELs, because these risks depend mainly on the unpredictable susceptibility of the cohorts. Although in theory there is no threshold in real concentration-response relationships, we demonstrated that a NOAEL approach would be a more robust and practical solution to the complex problem of setting standard.},
	title = {A Randomized Controlled Trial Assessing Infectious Disease Risks from Bathing in Fresh Recreational Waters in Relation to the Concentration of Escherichia coli, Intestinal Enterococci, Clostridium perfringens, and Somatic Coliphages},
	volume = {114},
	author = {Wiedenmann,A. and Krueger,P. and Dietz,K. and Lopez-Pila,J. M. and Szewzyk,R. and Botzenhart,K.},
	isbn = {0091-6765},
	journal = {Environ.Health Perspect.}
}

