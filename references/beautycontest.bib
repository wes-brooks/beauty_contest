@ARTICLE{Rosenstock:2005sv,
  author = {Rosenstock,S. S. and Bleich,V. C. and Rabe,M. J. and Reggiardo,C.},
  title = {Water Quality at Wildlife Water Sources in the Sonoran Desert, United
	States},
  journal = {Rangeland Ecology \& Management},
  year = {2005},
