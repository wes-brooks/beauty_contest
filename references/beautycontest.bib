	keywords = {Animals; Chlorine; Clostridium perfringens: pathogenicity; Cryptosporidium parvum: pathogenicity; Disinfection: methods; Electrochemistry; Escherichia coli: pathogenicity; Kinetics; Oocysts; Salts: chemistry; Vibrio cholerae: pathogenicity; Water Purification: methods},
	publisher = {50(1):141-6},
