	ty = {JOUR},
	date = {2010-12-23 11:37:42 -0600},
	pages = {302--309},
	keywords = {albendazole; ivermectin; tiabendazole; abdominal pain; Ancylostoma; bacterial superinfection; biology; case management; cat; developed country; developing country; diarrhea; differential diagnosis; dog; egg; enzyme linked immunosorbent assay; eosinophilic enteritis; epiluminescence microscopy; folliculitis; fomite; geographic distribution; hookworm; host; human; infection control; infection prevention; larva migrans; life cycle; Loeffler pneumonia; morbidity; muscle biopsy; myositis; nausea; nonhuman; papule; parasite migration; priority journal; pruritus; review; scratching; seasonal variation; skin; skin penetration; skin protection; sleep deprivation; soil pollution; Strongyloides stercoralis; systemic therapy; tourism; travel; treatment failure; tropics; Administration, Oral; Administration, Topical; Animals; Anthelmintics; Developed Countries; Developing Countries; Diagnosis, Differential; Hookworm Infections; Humans; Seasons; Soil},
	title = {Epidemiological and clinical characteristics of hookworm-related cutaneous larva migrans},
