}

@ARTICLE{Wade:2006qc,
  author = {Wade,T. J. and Calderon,R. L. and Sams,E. and Beach,M. and Brenner,K.
	P. and Williams,A. H. and Dufour,A. P.},
  title = {Rapidly Measured Indicators of Recreational Water Quality Are Predictive
	of Swimming-Associated Gastrointestinal Illness},
  journal = {Environmental health perspectives},
  year = {2006},
  volume = {114},
  pages = {24--28},
  number = {1},
  month = {Jan},
  address = {U.S. EPA Human Studies Division, MD 58A, Research Triangle Park,
	NC 27711 USA, {$[$}mailto:Calderon.rebecca{\char64}epa.gov{$]$}},
  annote = {Standard methods to measure recreational water quality require at
	least 24 hr to obtain results, making it impossible to assess the
	quality of water within a single day. Methods to measure recreational
	water quality in less than or equal to 2 hr have been developed.
	Application of rapid methods could give considerably more accurate
	and timely assessments of recreational water quality. We conducted
	a prospective study of beachgoers at two Great Lakes beaches to examine
	the association between recreational water quality, obtained using
	rapid methods, and gastrointestinal (GI) illness after swimming.
	Beachgoers were asked about swimming and other beach activities and
	10-12 days later were asked about the occurrence of GI symptoms.
	We tested water samples for Enterococcus and Bacteroides species
	using the quantitative polymerase chain reaction (PCR) method. We
	observed significant trends between increased GI illness and Enterococcus
	at the Lake Michigan beach and a positive trend for Enterococcus
	at the Lake Erie beach. The association remained significant for
	Enterococcus when the two beaches were combined. We observed a positive
	trend for Bacteroides at the Lake Erie beach, but no trend was observed
	at the Lake Michigan beach. Enterococcus samples collected at 0800
	hr were predictive of GI illness that day. The association between
	Enterococcus and illness strengthened as time spent swimming in the
	water increased. This is the first study to show that water quality
	measured by rapid methods can predict swimming-associated health
	effects.},
