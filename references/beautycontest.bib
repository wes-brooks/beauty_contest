	m = {Journal Article},
	title = {Impact of the alga Cladophora on the survival of E. coli, Salmonella, and Shigella in laboratory microcosm},
	keywords = {Beach monitoring; Cladophora; E. Coli; Quality; Recreational water; Salmonella; Shigella; Algae; Beaches; Contamination; Lake pollution; Lakes; Landforms; Phytoplankton; Population statistics; Water pollution; Elevated concentrations; Fecal contamination; Great Lakes (CO); Green algae; indicator organisms; Laboratory microcosms; lake water; Pathogenic organisms; Time periods; Escherichia coli; bacterium; beach; bioindicator; environmental monitoring; fecal coliform; green alga; laboratory method; microcosm; persistence; survival; Great Lakes {$[$}North America{$]$}; North America; Bacteria (microorganisms); Chlorophyta},
	number = {2},
