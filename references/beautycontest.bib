@article{Steets:2003ro,
	ty = {JOUR},
	number = {3},
	isbn = {00431354},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0037211591&partnerID=40&rel=R8.0.0},
	pages = {589--608},
	title = {A mechanistic model of runoff-associated fecal coliform fate and transport through a coastal lagoon},
	address = {Affiliation: Dept. of Mechanical Engineering, University of California, Santa Barbara, CA 93106, United States; Affiliation: D. Bren Sch. Environ. Science/Mgmt., University of California, Donald Bren Hall, Santa Barbara, CA 93106, United States;(TRUNCATED)},
	annote = {Fecal coliform (FC) contamination in coastal waters is an ongoing public health problem worldwide. Coastal wetlands and lagoons are typically expected to protect coastal waters by attenuating watershed pollutants including FC bacteria. However, new evidence suggests that coastal lagoons or marshes can also be a source of high indicator organism concentrations in coastal waters. We asked for a Mediterranean-type climate, what is the fate of runoff-associated FC through a coastal lagoon? To address this question, we developed a mass balance-based, mechanistic model of FC concentration through a coastal lagoon and simulated, for summer and winter conditions, FC within the lagoon water column, lagoon sediments, and in the ocean water just downstream of the lagoon mouth. Our model accounts for advective flow and dispersion, decay and sedimentation and resuspension of FC-laden sediments during high flow, erosional conditions. Under low flow conditions that occur in the summer, net FC decay and FC storage in lagoon sediments are predicted. Under high flow conditions that occur in the winter, FC-laden sediments are predicted to erode, resuspend and flow out of the lagoon where they elevate FC concentrations in the coastal ocean. For both seasonal conditions, the predicted water column FC concentrations were within an order of magnitude of field measurements for a reference site in southern California. Our results suggest that there are seasonally varying roles for coastal lagoons in mediating FC contamination to coastal waters. {\copyright}2002 Elsevier Science Ltd. All rights reserved.},
	volume = {37},
	author = {Steets,B. M. and Holden,P. A.},
	keywords = {Coastal; Estuary; Fecal coliform; Lagoon; Non-point source; Sediment; Water quality; Watershed; Bacteria; Coastal zones; Computer simulation; Contamination; Health; Sedimentation; Sewage lagoons; Watersheds; Mass balance; Runoff; sea water; coastal lagoon; pollutant transport; article; bacterium contamination; climate; dispersion; erosion; feces microflora; mathematical model; particle resuspension; prediction; priority journal; sea pollution; seashore; seasonal variation; simulation; Southern Europe; storage; summer; United States; water contamination; water flow; winter; Ecosystem; Enterobacteriaceae; Feces; Geologic Sediments; Mediterranean Region; Models, Theoretical; Public Health; Seasons; Water Microbiology; Water Movements},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/steets_2003_mechanistic_model_runoff_fecal_coliform_fate_transport_coastal_lagoon.pdf},
	year = {2003},
	m = {Journal Article},
	journal = {Water Research},
	date = {2010-12-23 11:37:44 -0600}
}

