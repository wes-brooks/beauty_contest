	date = {2010-12-23 11:37:42 -0600},
	m = {Journal Article},
	ty = {JOUR},
	keywords = {Coast; Marine; Microbial pollution; Sediments; amphotericin B; antibiotic agent; penicillin derivative; streptomycin; marine sediment; microbial community; sediment pollution; article; bacterium detection; bacterium isolation; coliform bacterium; enteric virus; Enterococcus; Italy; marine environment; nonhuman; Salmonella; seashore; seasonal variation; sediment; Vibrio; virus detection; virus isolation; water contamination; water pollution; Bathing Beaches; Disease Reservoirs; Enterobacteriaceae; Geologic Sediments; Humans; Mediterranean Sea; Sewage; Adriatic Coast {$[$}Italy{$]$}; Eurasia; Europe; Southern Europe; Bacteria (microorganisms)},
