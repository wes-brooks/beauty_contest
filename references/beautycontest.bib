	title = {Vibrio vulnificus infections in Denmark during the summer of 1994},
	number = {30},
	keywords = {antibody; sea water; adolescent; adult; aged; article; child; Denmark; epidemic; female; Gram negative infection; human; male; middle aged; risk factor; season; swimming; temperature; Antibodies; Bathing Beaches; Disease Outbreaks; Humans; Risk Factors; Seasons; Seawater; Vibrio Infections},
