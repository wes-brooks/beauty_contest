@ARTICLE{Epstein:1995nz,
  author = {Epstein,S. S. and Rossel,J.},
  title = {Enumeration of sandy sediment bacteria: search for optimal protocol},
  journal = {Marine Ecology Progress Series},
  year = {1995},
  volume = {117},
  pages = {289--298},
  month = {February 9},
  annote = {We examined and compared a variety of existing protocols for enumeration
	of bacteria from marine sandy sediments. The focus was on how to
	dislodge bacteria from sediment particles; a commercial blender,
	an ultrasonic cleaner, and an ultrasonic cell disrupter were tested.
	The ultrasonic cell disrupter was found to be the most efficient
	device for bacterial dislodgment. With a 5 mm microtip vibrating
	at 109 amplitude and 20 kHz, the optimal sonication time of small
	(<0 5 cm3) samples was 180 s. Having identified the optimal dislodgment
	treatment, we went through other steps of sediment bacteria enumeration
	(use of surfactants, thoroughness of sample washing, bacterial staining
	and counterstaining, etc.), striving for additional ways of improvement.
	The result was a protocol that, in our study, gave bacterial counts
	superior to those obtained by any other protocol currently in use.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/epstein_1995_enumeration_sandy_sediment_bacteria_search_optimal_protocol.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
