	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/ferguson_2005_enumeration_speciation_enterococci_marine_intertidal_sediments_coastal_water_california.pdf},
	number = {3},
	pages = {598--608},
	title = {Enumeration and speciation of enterococci found in marine and intertidal sediments and coastal water in southern California},
	keywords = {Beach pollution; Enterococci; Faecal indicator bacteria; Marine sediments; Water quality; coastal water; marine sediment; pollution; article; bacterium identification; bacterium isolate; coastal waters; Enterococcus; Enterococcus casseliflavus; Enterococcus faecalis; Enterococcus faecium; Enterococcus hirae; enterococcus mundtii; intertidal zone; membrane filter; nonhuman; river; sediment; species differentiation; United States; California; Colony Count, Microbial; Environmental Pollution; Feces; Geologic Sediments; Oceans and Seas; Rivers; Streptococcus; Water Microbiology; Water Pollution; North America; Western Hemisphere; World}
