	keywords = {WATER POLLUTION - Mathematical Models; NAVAL VESSELS; environmental health},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0016468462&partnerID=40&rel=R8.0.0},
	year = {1975},
