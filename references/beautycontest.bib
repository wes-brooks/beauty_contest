@article{Alm:2006gm,
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-33746132338&partnerID=40},
	number = {2},
	m = {Journal Article},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/alm_2006_persistence_potential_growth_ecoli_lake_huron.pdf},
	year = {2006},
	date = {2010-12-23 11:37:42 -0600},
	pages = {401--405},
	address = {Affiliation: Department of Biology, Central Michigan University, 157 Brooks Hall, Mount Pleasant, MI 48859, United States; Affiliation: Faculty of Forestry and the Forest Environment, Lakehead University, Thunder Bay, Ont., United States; Affili(TRUNCATED)},
	title = {Persistence and potential growth of the fecal indicator bacteria, Escherichia coli, in shoreline sand at Lake Huron},
	isbn = {03801330},
	annote = {Recent investigations found high abundances of the fecal indicator Escherichia coli in shoreline sand at freshwater beaches, but it is not known whether these high numbers are due to passive filtration/trapping of the bacteria, or to colonization and growth. This study was initiated to test the hypothesis that high abundance can be explained, at least in part, by the ability of E. coli to persist and grow in beach sand. A combination of laboratory and field studies was used to monitor the densities of environmental isolates of E. coli in beach sand. In controlled laboratory microcosm studies using autoclaved beach sand inoculated with E. coli strains previously isolated from ambient beach sand, E. coli densities increased from 2 CFU/g to more than 2 ×105 CFU/g sand after 2 days of incubation at 79$\,^{\circ}$C, and remained above 2 ×105 CFU/g for at least 35 days. In field studies utilizing similarly inoculated beach sand in diffusion chambers incubated at a Lake Huron beach, E. coli also grew rapidly, reaching high densities (approximately 7.5 ×105 CFU/g), and persisting in a cultivable state at high density for at least 48 days. In comparison, E. coli levels in ambient beach sand adjacent to the chambers always had densities <100 CFU/g. Lake Huron beach sand clearly provides nutrients, temperatures, and other conditions needed to support growth of E. coli. The growth of E. coli in sterile sand diffusion chambers to higher levels than occurs in ambient beach sand may indicate the presence in ambient sand of biological controls on bacterial growth, such as predation or competition.},
	volume = {32},
	author = {Alm,E. W. and Burke,J. and Hagan,E.},
	journal = {J.Great Lakes Res.},
	ty = {JOUR},
	keywords = {Beach monitoring; E. coli; Fecal indicator; Growth; Persistence; Sand; Water quality; Bacteria; Beaches; Diffusion; Filtration; Growth kinetics; Thermal effects; Escherichia coli; abundance; biological control; fecal coliform; freshwater; hypothesis testing; isolation effect; microcosm; population growth; shore (nonmarine); shoreline; Great Lakes; Lake Huron; North America; Bacteria (microorganisms)}
}

