@INPROCEEDINGS{Klemas:1990fn,
  author = {Klemas,V. and Lyzenga,D. and Matteoda,A. and Bostater,C.},
  title = {New airborne sensors for monitoring estuaries and coastal waters
	in conjunction with satellites},
  booktitle = {Conference Proceedings - OCEANS '90},
  year = {1990},
  volume = {Washington, DC, USA},
  pages = {422--427},
  address = {Piscataway, NJ, United States},
  month = {24 September 1990 through 26 September 1990},
  publisher = {Publ by IEEE},
  address1 = {Affiliation: Coll of Marine Studies, Univ of, Delaware, Newark, DE,
	USA; Correspondence Address: Klemas, V.; Coll of Marine Studies,
	Univ of; Delaware, Newark, DE, USA},
  annote = {Satellite sensors alone cannot provide the temporal and spatial resolutions
	required by watershed, hydrodynamic, water quality, and living resource
	models. Aircraft can provide frequent overflights at good spatial
	resolution, but large four-engine aircraft are too expensive to be
	flown repeatedly. A sensor package is being developed which will
	fit on small aircraft at a ten-time reduction in operating cost.
	The sensor package will include a small multispectral camera for
	water color measurements from which chlorophyll, suspended sediment,
	and dissolved organic concentrations will be estimated using newly
	developed algorithms and models. A PRT-5 radiometer will measure
	surface temperature and a microwave radiometer, which is being reduced
	in size, will measure water salinity. Deployed with satellite sensors,
	these airborne sensors will be able to observe tidal, seasonal, annual
	variations and spatial distributions of phytoplankton blooms, sediment
	plumes, estuarine fronts, circulation patterns, and other estuarine
	phenomena. An optical model is briefly described which has been developed
	for measuring the concentrations of suspended sediment, chloropyll,
	and dissolved organics in turbid estuarine waters. An airborne instrument
	package which is being developed to meet the model requirements is
	discussed.},
  annote1 = {Sponsors: IEEE Oceanic Engineering Soc; Conference code: 14774},
