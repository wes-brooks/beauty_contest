@ARTICLE{Whitman:2008nb,
  author = {Whitman,R. L. and Nevers,M. B.},
  title = {Summer E. coli Patterns and Responses along 23 Chicago Beaches},
  journal = {ENVIRONMENTAL SCIENCE \& TECHNOLOGY},
  year = {2008},
  volume = {42},
  pages = {9217--9224},
  number = {24},
  annote = {Concentrations of E. coli in recreational beach water are highly
	variable both locally and temporally, but a broader understanding
	of these fluctuations may be explained through coastal observations.
	Currently, beach contamination study approaches tend to be site-specific
	under the belief that politically delineated beaches are unique and
	management of beaches cannot be regionally oriented. E. coli data
	collected over five years from 23 Chicago beaches clearly identified
	ambient linked patterns at the regional scale. Temporal fluctuations
	were similar, with all beaches having simultaneous peaks and troughs
	of E. coli concentrations. Spatially, E. coli concentrations for
	beaches more closely situated were more closely correlated, indicating
	spatial autocorrelation. Julian day, wave height, and barometric
	pressure explained up to 40% of the variation,avalue comparable to
	individual, less parsimonious sitespecific models. Day of sampling
	could explain the majority of the variation in E. coli concentrations,
	more so than beach, depth, or time of day. Comparing beaches along
	a targeted coastline allows a better understanding of inherent background
	regional fluctuations and, ultimately, better predictions of E. coli
	concentrations in coastal recreational water.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitman_2008_summer_ecoli_patterns_responses_23_chicago_beaches.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:44 -0600},
  journal1 = {Environ. Sci. Technol.},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitman_2008_summer_ecoli_patterns_responses_23_chicago_beaches.pdf}
}

@TECHREPORT{Whitman:2005qw,
