  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:43 -0600},
  keywords = {atmospheric precipitation; bacteria; beaches; case studies; coastal
	environment; coliform bacteria; Escherichia; Escherichia coli; Great
	Lakes; Huntington Beach; indicators; Lake Erie; models; North America;
	Ohio; pollutants; pollution; prediction; public health; recreation;
	risk assessment; United States; USGS; water quality},
  m1 = {Report},
  publisher = {U. S. Geological Survey, Reston, VA, United States (USA)},
  sp = {34},
