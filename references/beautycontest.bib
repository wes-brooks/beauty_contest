  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:44 -0600},
  keywords = {multivariate statistics; functional MRI; event-related potentials;
	positron emission tomography; magnetoencephalography; nonparametric;
	statistics},
  m1 = {Conference Proceedings},
  ty = {CONF},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/mcintosh_2004_partial_least_squares_analysis_neuroimaging_data.pdf}
}

