@ARTICLE{Nevers:2007kt,
  author = {Nevers,M. B. and Whitman,R. L. and Frick,W. E. and Ge,Z.},
  title = {Interaction and influence of two creeks on Escherichia coli concentrations
	of nearby beaches: exploration of predictability and mechanisms},
  journal = {Journal of environmental quality},
  year = {2007},
  volume = {36},
  pages = {1338--1345},
  number = {5},
