@article{Mimouni:2002aa,
	author = {Mimouni,R. and Ait Alla,A. and Anajjar,E. M. and Finance,C. and Moukrim,A.},
	m = {Journal Article},
	journal = {J.Eur.Hydrol.},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-27744548866&partnerID=40},
	year = {2002},
	ty = {JOUR},
	date = {2010-12-23 11:37:44 -0600},
