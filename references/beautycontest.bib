  date-modified = {2010-12-23 11:37:44 -0600},
  keywords = {Quantitative PCR; Propidium monoazide; Enterococcus; Bacteroidales;
	Wastewater treatment efficacy},
  m1 = {Journal Article},
  ty = {JOUR}
}

