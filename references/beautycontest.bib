	m = {Journal Article},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/ogan_1994_improved_recovery_fecal_streptococci_tropical_recreational_waters_MF.pdf},
	year = {1994},
	date = {2010-12-23 11:37:44 -0600},
	journal = {WATER AIR SOIL POLLUT.},
	keywords = {Beaches; Coliform bacteria; Contamination; Hydrolysis; Marine pollution; Recreational facilities; Sampling; Water analysis; Water filtration; Water quality; Anaerobic membrane filtration; Brain heart infusion agar; Fecal streptococci; m-Enterococcus agar; Most probable number; Water bacteriology; sea water; article; bacterium contamination; controlled study; feces microflora; recreation; streptococcus; tropics; bacteria collecting devices; coastal water quality; faecal streptococci; microbial contamination; water quality analysis; Barbados},
