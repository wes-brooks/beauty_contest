	annote = {In a preliminary study during the summer of 1963, an examination of adults of the biting stable fly, Stomoxys calcitrans (Linnaeus), from a farm, a beach near the farm, and a distant beach showed that few harbored the bacterium Escherichia coli (Migula) Castellani and Chalmers (5.0 percent of the total of 300 flies examined). More flies were positive for bacteria at the farm than at the beaches. Enterococci were found in 19.7 percent of the total of 295 flies examined and their frequency was also greatest among farm flies. In a more extensive study during the summer of 1964 coliforms were found in 10.5 percent of 1293 flies examined and E. coli in 6.1 percent of these flies. Enterococci were isolated from 18.0 percent of 1307 flies. In farm flies coliforms and enterococci were found in 21.6 and 32.8 percent respectively, of the flies tested, whereas at a beach 3 4 mile from the farm the incidence dropped to 6.8 percent for coliforms and 11.6 percent for enterococci. At a beach 8 miles from the farm coliforms occurred in 3.6 percent and enterococci in 13.8 percent of the flies tested. {\copyright}1965.},
	volume = {7},
	isbn = {00222011},
	author = {Love,J. A. and Gill,G. D.},
	journal = {J.Invertebr.Pathol.}
}

@article{Lowe:2008sw,
