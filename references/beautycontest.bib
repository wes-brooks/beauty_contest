  journal1 = {Science},
  keywords = {aerosol; bacteriophage; epidemiology; in vitro study; microorganism;
	theoretical study; Aerosols; Air Microbiology; Coliphages; Support,
	U.S. Gov't, Non-P.H.S.; Viruses; Water Microbiology; Water Movements;
	Wind},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/baylor_1977_virus_transfer_surf_to_wind.pdf},
