	title = {Use of neural networks in design of coastal sewage systems},
	keywords = {Coastal zones; Coliform bacteria; Mathematical models; Neural networks; Optimization; Runoff; Wastewater; Water quality; Coastal sewage systems; Storm sewers},
	number = {5},
	address = {Affiliation: Esc. Ingenieros Caminos Canales y P., Universidad de Cantabria, Dto. Matemat. Apl. y Cie. la Comp., Av. de los Castros s/n, 39005. Santander, Spain; Affiliation: Confederacion Hidrografica del N., Juan de Herrera 1, 39007. Santander(TRUNCATED)},
