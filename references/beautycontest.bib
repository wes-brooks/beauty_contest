@article{Whitman:2008nb,
	m = {Journal Article},
	number = {24},
	journal = {ENVIRONMENTAL SCIENCE \& TECHNOLOGY},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitman_2008_summer_ecoli_patterns_responses_23_chicago_beaches.pdf},
	pages = {9217--9224},
	year = {2008},
	title = {Summer E. coli Patterns and Responses along 23 Chicago Beaches},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitman_2008_summer_ecoli_patterns_responses_23_chicago_beaches.pdf},
	date = {2010-12-23 11:37:42 -0600},
	annote = {Concentrations of E. coli in recreational beach water are highly variable both locally and temporally, but a broader understanding of these fluctuations may be explained through coastal observations. Currently, beach contamination study approaches tend to be site-specific under the belief that politically delineated beaches are unique and management of beaches cannot be regionally oriented. E. coli data collected over five years from 23 Chicago beaches clearly identified ambient linked patterns at the regional scale. Temporal fluctuations were similar, with all beaches having simultaneous peaks and troughs of E. coli concentrations. Spatially, E. coli concentrations for beaches more closely situated were more closely correlated, indicating spatial autocorrelation. Julian day, wave height, and barometric pressure explained up to 40% of the variation,avalue comparable to individual, less parsimonious sitespecific models. Day of sampling could explain the majority of the variation in E. coli concentrations, more so than beach, depth, or time of day. Comparing beaches along a targeted coastline allows a better understanding of inherent background regional fluctuations and, ultimately, better predictions of E. coli concentrations in coastal recreational water.},
	volume = {42},
	author = {Whitman,R. L. and Nevers,M. B.},
	ty = {JOUR}
}

@techreport{Whitman:2005qw,
	m = {Report},
	ty = {RPRT},
	pages = {38--38},
	title = {Recreational Water Quality of Door County Beaches 2003-2004: A Study of Factors Affecting E. coli Occurrence},
	annote = {INTRODUCTION The following report addresses the E. coli monitoring data collected in the summers of 2003 and 2004 for Door County, Wisconsin. E. coli data were collected at 29 beach locations along both sides of the peninsula, at Washington Island, within Sturgeon Bay, and at inland lakes. Additionally, several beaches were more intensively sampled on several occasions, including Ephraim, Fish Creek, Otumba, Sister Bay, and Whitefish Dunes beaches. Spatial sampling (depth of water and location on the beach) was conducted at these beaches typically three days a week throughout the summer sampling period. Ambient conditions in the field were recorded, including wind conditions, number of gulls present on the beach, and abundance of amassed Cladophora algae along the shoreline. Daily rainfall data were collected at 3 locations on the peninsula: Sturgeon Bay, Egg Harbor, and Sister Bay. Additional ambient weather data were collected at a NOAA-operated buoy located at the north end of Lake Michigan, near the north end of the Door County peninsula. This report includes a descriptive analysis of the dataset collected for Door County; it includes comparisons of E. coli results from the beaches. It also provides analysis of the spatial sampling and comparisons of E. coli counts with ambient conditions. Analysis of rainfall effects on E. coli are included, with several rainfall sampling events examined intensively. Land use, beach slope, beach substrate, and sampling location were also considered relative to E. coli counts at certain beaches. Finally, regression and predictive modeling was explored for groups of Door County beaches to determine what parameters could be used as surrogates for E. coli.},
	author = {Whitman,R. L. and Nevers,M. B.},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitman_2005_recreational_QW_door_county_beaches_2003-4_ecoli_occurrence.pdf},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitman_2005_recreational_QW_door_county_beaches_2003-4_ecoli_occurrence.pdf},
	year = {2005},
	date = {2010-12-23 11:37:44 -0600},
	month = {June 21, 2005}
}

