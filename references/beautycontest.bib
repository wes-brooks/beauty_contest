@article{Hsu:1995ui,
	journal = {APPLIED AND ENVIRONMENTAL MICROBIOLOGY},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/hsu_1995_genotyping_male-specific_RNA_coliphages_hybridization_oligonucleotide_probes.pdf},
	number = {11},
	m = {Journal Article},
	month = {Nov. 1995},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/hsu_1995_genotyping_male-specific_RNA_coliphages_hybridization_oligonucleotide_probes.pdf},
	year = {1995},
	ty = {JOUR},
	date = {2010-12-23 11:37:43 -0600},
	annote = {F-specific (F1) RNA coliphages are prevalent in sewage and other fecal wastes of humans and animals. There are four antigenically distinct serogroups of F1 RNA coliphages, and those predominating in humans (groups II and III) differ from those predominating in animals (groups I and IV). Hence, it may be possible to distinguish between human and animal wastes by serotyping F1 RNA coliphage isolates. Because serotyping is laborious and requires scarce antiserum reagents, we investigated genotyping using synthetic oligonucleotide probes as an alternative approach to distinguishing the four groups of F1 RNA coliphages. Oligoprobes I, II, III, IV, A, and B were selected to detect group I, II, III, IV, I plus II, and III plus IV phages, respectively. Methods for phage transfer from zones of lysis on a host cell lawn to candidate membrane filters and fixation of genomic nucleic acid on the membranes were optimized. The oligoprobes, which were end labeled with digoxigenin, were applied in DNA-RNA hybridizations, and hybrids were observed by colorimetric, immunoenzymatic detection. Of 203 isolates of F1 RNA coliphages from environmental samples of water, wastes, and shellfish, 99.5 and 96.6% could be classified into each group by serotyping and genotyping, respectively. Probes A and B correctly identified 100% of the isolates. On the basis of these results, this method for genotyping F1 RNA coliphages appears to be practical and reliable for typing isolates in field samples.},
	volume = {61},
	author = {Hsu,F. -C and Shieh,Y. -S C. and van Duin,J. and Beekwilder,M. J. and Sobsey,M. D.},
	isbn = {0099-2240/95},
	pages = {3960--3966},
	title = {Genotyping Male-Specific RNA Coliphages by Hybridization with Oligonucleotide Probes}
}

@article{Hubas:2007nl,
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-33847397527&partnerID=40},
	pages = {41--52},
	title = {Seasonal variability of intertidal bacterial metabolism and growth efficiency in an exposed sandy beach during low tide},
	ty = {JOUR},
	address = {Affiliation: FRE 2816, Universit{\'e}du Littoral C{\^o}te d'Opale, Maison de la Recherche en Environnement Naturel, 32 av. Foch, Dunkerque Cedex 62930, France; Affiliation: UMR 7144 AD2M, Universit{\'e}Pierre et Marie Curie-Paris6, Station Biologiqu(TRUNCATED)},
