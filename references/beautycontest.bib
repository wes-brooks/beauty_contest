@article{Futch:2010rc,
	journal = {Environmental Microbiology},
	pages = {964--974},
	title = {Human enteric viruses in groundwater indicate offshore transport of human sewage to coral reefs of the Upper Florida Keys},
	m = {Journal Article},
	number = {4},
	month = {April 2010},
	year = {2010},
	ty = {JOUR},
	date = {2010-12-23 11:37:42 -0600},
	annote = {To address the issue of human sewage reaching corals along the main reef of the Florida Keys, samples were collected from surface water, groundwater and coral {$[$}surface mucopolysaccharide layers (SML){$]$} along a 10 km transect near Key Largo, FL. Samples were collected semi-annually between July 2003 and September 2005 and processed for faecal indicator bacteria (faecal coliform bacteria, enterococci and Clostridium perfringens) and human-specific enteric viruses (enterovirus RNA and adenovirus DNA) by (RT)-nested polymerase chain reaction. Faecal indicator bacteria concentrations were generally higher nearshore and in the coral SML. Enteric viruses were evenly distributed across the transect stations. Adenoviruses were detected in 37 of 75 samples collected (49.3{\%}) whereas enteroviruses were only found in 8 of 75 samples (10.7{\%}). Both viruses were detected twice as frequently in coral compared with surface water or groundwater. Offshore, viruses were most likely to be found in groundwater, especially during the wet summer season. These data suggest that polluted groundwater may be moving to the outer reef environment in the Florida Keys.},
	volume = {12},
	author = {Futch,J. C. and Griffin,D. W. and Lipp,E. K.}
}

@article{Fera:2002nk,
	m = {Journal Article},
	ty = {JOUR},
	annote = {The restoration of the quality - especially the microbiological aspect - and usage of coastal water has always been and will remain one of the main concerns and objectives of the Loire Bretagne Water Agency. In that respect, the implementation of its action plans - right from its creation - has made it possible to maintain and restore the usage quality levels that meet national and European standards. The Loire Bretagne Agency, which manages the longest coastline of all local authorities in France, serves a twofold objective: to improve the sanitary quality of the bathing and conchyliculture water on the one hand, and to fight against the proliferation of phytoplanctons - toxic or non toxic - and macro green algae along the coastline of Brittany on the other hand. Indeed, due to its natural characteristics and to intensive farming, Brittany is severely affected by "green tides". Within the framework of the 5th and 6th programmes, major efforts were devoted to the construction of collection and purification works and facilities for the coastal local authorities - especially the municipalities - the main objective thereof being the restoration of the sanitary quality of the beaches. This policy has been supported by means of reduced rate financial aid to compensate for increased fees. During this period, the Agency carried out studies intended to gain a better knowledge and understanding of the dynamics and phenomena of the environment. Within the framework of the 7th programme, the Agency reinforces its sanitary quality improvement policy for bathing water and extends it to conchyliculture; moreover it proposes aids with a 20{\%} increase in that direction. It also develops a policy against pollution based on geographical zones, especially to minimize the proliferation of green algae. Preferential financings are elaborated in colloboration with the Bretagne Regional Authority for pilot catchment basins. Agricultural pollution control is one of Brittany most urgent challenge. This fight is supported by the fact that this region has just been classified as a vulnerable zone within the framework of the European Directive on nitrates and through the implementation of various regional programmes. Continuous efforts are in progress in order to improve the knowledge of the environment and of the phenomena that affect it, particularly the toxic micro algae. Finally, a very dynamic concerted political approach (SAGE - bay contract : restoration-preservation contract), is implemented to manage the coastline under the authority of the Agency.},
	author = {F{\'e}ra,P.},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0036222838&partnerID=40},
	year = {2002},
	journal = {Tech.Sci.Methodes},
	date = {2010-12-23 11:37:43 -0600},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0036222838&partnerID=40},
	isbn = {02997258},
	number = {2},
	pages = {27--32},
	title = {Coastal sanitation. 30 years of action: Successes and evolutions},
	keywords = {coastal zone; pollution control; water quality; water treatment; coastal water; sanitation; France}
}

