@article{Sampson:2006hg,
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-32044457157&partnerID=40},
	pages = {151--159},
	title = {The effects of rainfall on Escherichia coli and total coliform levels at 15 Lake Superior recreational beaches},
	m = {Journal Article},
	address = {Affiliation: Department of Biology and Microbiology, University of Wisconsin-Oshkosh, 800 Algoma Boulevard, Oshkosh, WI 54901, United States; Correspondence Address: Kleinheinz, G.T.; Department of Biology and Microbiology; University of Wiscons(TRUNCATED)},
