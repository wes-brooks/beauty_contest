  keywords = {Cryptosporidium; Giardia; enteric viruses; water quality; catchment;
	indicator organisms},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/ferguson_2003_fate_transport_SW_pathogens_watersheds.pdf}
}

