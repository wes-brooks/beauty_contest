	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-33947263010&partnerID=40&rel=R8.0.0},
	keywords = {Diseases; Ecosystems; Wastewater; Wastewater treatment; Watersheds; Campylobacter; Diarrhea; Food borne diseases; Bacteria; ammonia; nitrate; phosphate; bacterium; coastal plain; diarrheal disease; disease prevalence; domestic waste; food poisoning; health risk; human activity; rural area; streamwater; waste treatment; water quality; watershed; agriculture; article; bacterial count; bacterial transmission; bacterium contamination; bacterium isolation; concentration (parameters); conductance; developed country; environmental exposure; food contamination; multivariate analysis; nonhuman; nutrient; pH; poultry; precipitation; prevalence; risk factor; seashore; seasonal variation; sewage; statistical model; stream (river); summer; thermophilic bacterium; United States; waste water treatment plant; wildlife; Abattoirs; Animals; Colony Count, Microbial; Ecosystem; Georgia; Humans; Rivers; Seasons; Waste Disposal, Fluid; Water Pollution; North America; Animalia; Bacteria (microorganisms)},
	address = {Affiliation: University of Georgia, Department of Environmental Health Science, Athens, GA 30602, United States; Affiliation: USDA Agricultural Research Service, Southeast Watershed Research Lab., Tifton, GA 31793, United States; Affiliation: Un(TRUNCATED)},
	annote = {Campylobacter is the leading cause of bacterium-associated diarrhea in the United States and most developed countries. While this disease is considered a food-borne disease, many clinical cases cannot be linked to a food source. In rural and agrarian areas environmental transmission may be an important factor contributing to case loads. Here we investigated the waterborne prevalence of campylobacters in a mixed-use rural watershed in the coastal plain of southern Georgia (United States). Six sites representing various degrees of agricultural and human influence were surveyed biweekly to monthly for 1 year for the presence of culturable thermophilic campylobacters and other measures of water quality. Campylobacters were frequently present in agriculture-and sewage-impacted stretches of streams. The mean campylobacter counts and overall prevalence were highest downstream from a wastewater treatment plant that handled both human and poultry slaughterhouse waste (≤595 CFU ml -1; 100{\%} positive); the concentrations were significantly higher than those for the four upstream sites (P < 0.05). The counts were significantly correlated with the number of fecal coliform bacteria, conductivity, pH, and concentrations of nutrients (NO3-, PO43-, and NH3). Campylobacters were isolated more frequently and larger numbers were present during the summer months, similar to the occurrence of clinical cases of campylobacteriosis in this region. A multivariate model showed that the levels were significantly influenced by increasing precipitation, which also peaked in the summer. The results indicate that loading from both human and domestic animal waste may be high in the watershed studied during the summer months. Mixed-use watersheds supporting agriculture production, human populations, and wildlife may be at risk for contamination by campylobacters and may be an important route for human exposure. Copyright {\copyright}2007, American Society for Microbiology. All Rights Reserved.},
	volume = {73},
	author = {Vereen Jr. ,E. and Lowrance,R. R. and Cole,D. J. and Lipp,E. K.},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/vereen_2007_distribution_ecology_campylobacters_coastal_plain_streams_georgia_usa.pdf},
	year = {2007},
	date = {2010-12-23 11:37:44 -0600},
	pages = {1395--1403},
	title = {Distribution and ecology of Campylobacters in coastal plain streams (Georgia, United States of America)}
}

@article{Verhoeven:2002iq,
	volume = {46},
	date = {2010-12-23 11:37:42 -0600},
	author = {Verhoeven,R.},
	journal = {Pedobiologia},
	m = {Journal Article},
	keywords = {Coastal dunes; Microfauna; Microtrophic system; Primary succession; Soil food web; abundance; community structure; dune; food web; soil fauna; Bacteria (microorganisms); Ciliophora; Nematoda; Rotifera; Testacealobosia},
	isbn = {00314056},
	number = {1},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0036124556&partnerID=40},
	ty = {JOUR},
	address = {Affiliation: University of Bremen, Centre for Environmental Research and Technology, Leobener Strasse, D-28359 Bremen, Germany; Correspondence Address: Verhoeven, R.; University of Bremen; Ctr. for Environ. Res./Technology; Leobener Strasse D-28(TRUNCATED)},
	pages = {75--89},
	title = {The structure of the microtrophic system in a development series of dune soils},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/verhoeven_2002_structure_microtrophic_system_development_series_dune_soils.pdf},
	year = {2002},
	annote = {In coastal dunes the structure of the microtrophic system was investigated on the basis of the relationship between testaceans, ciliates, nematodes, and rotifers within the microfaunal community. The eight investigated sites follow a gradient of soil development on coastal dune sand. Abundance of microfauna increased from sandy beach to forest and heath sites, dividing the sites into two distinct groups of successional stages: the young sites (beach, primary and white dune/grey dune) and the more mature sites (old grey dune, shrub, dune heath, and forest). The content of organic matter in soil, also influencing water conditions and pH, was the main factor with respect to abundance of all microfaunal groups. Testaceans were highly dominant in all sites except the beach and account for up to 95 {\%} of the total microfauna abundance. While ciliates were more abundant than nematodes in the young soils, the latter became more important in the soils with an accumulation of organic matter, where also rotifers occurred in higher numbers. The investigated microfaunal groups represent the higher trophic levels of the microtrophic system. The results show an increasing complexity of food web interactions and more trophic levels in older sites than in early successional stages. The changes in the soil properties and the energy source lead to a shift in the food web structure from bacterial dominated to detritus dominated nutrition and a higher amount of fungal feeding during soil development.}
}

