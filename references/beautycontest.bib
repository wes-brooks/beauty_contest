@article{Palmer:1993dg,
	m = {Journal Article},
	volume = {59},
	author = {Palmer,C. J. and Tsai,Y. -L and Paszko-Kolva,C. and Mayer,C. and Sangermano,L. R.},
	ty = {JOUR},
	keywords = {article; bacterium culture; bacterium detection; controlled study; culture medium; fluorescent antibody technique; legionella; legionella pneumophila; nonhuman; polymerase chain reaction; priority journal; river; sea; seashore; sewage; waste water; Bacteriological Techniques; Colony Count, Microbial; Disease Reservoirs; DNA, Bacterial; Evaluation Studies; Human; Legionellosis; Seawater; Support, Non-U.S. Gov't; Water Microbiology; bacterial culture; detection methods; direct fluorescent- antibody staining},
