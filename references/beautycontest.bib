  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/whitehead_2000_geotechnical_aspectes_domestic_on-site_effluent_management_systems.pdf},
  url1 = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0034144542&partnerID=40}
