@article{Ramalho:2002ak,
	number = {1},
	isbn = {0167-7012},
	keywords = {Article Subject Terms: Agar; Counting methods; Media (differential); Media (selective); Mineral water; Pigments; Pollution detection; Water quality; Article Taxonomic Terms: Pseudomonas aeruginosa; Pseudomonas Agar Base; cetrimide; enumeration; ferrous sulfate; nalidixic acid; pyocyanin},
	publisher = {Elsevier Science},
