	keywords = {biodegradation; conference paper; environmental health; human; Italy; methodology; microbiology; sewage; USSR; water pollution; Congresses; English Abstract; Waste Disposal, Fluid; Water Microbiology; Water Pollution, Chemical},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0024657790&partnerID=40},
