@book{Alexander:1991kk,
	pages = {591--599},
	title = {Recreational water quality and health: Can better management promote better health for all?},
	ty = {BOOK},
	booktitle = {Proceedings of International Conference on Environmental Pollution, Lisbon, April 1991. (Volume 2), Lisbon (Portugal), 15-19 April 1991},
	m = {Book, Whole},
	year = {1991},
	date = {2010-12-23 11:37:42 -0600},
	keywords = {Article Subject Terms: perception; water quality standards; bathing; recreation areas},
	address = {Environ. Epidemiol. Res. Unit, Lancaster Univ., Lancaster, UK},
	annote = {The effects of water quality on those who live in areas where malnutrition, schistosomiasis and early infant mortality are the norm rather than the exception are not in dispute. In the European Community, where the health of the population is generally better, and the standard of living higher, the issues surrounding recreational water quality are many and fraught with controversy. In this paper, we attempt to address these questions and others, while highlighting the necessity for an urgent review of bathing water quality standards. The incidence of illness amongst recreational water quality users is, however, only part of a much wider issue, namely that of water quality management. Aesthetic issues aside, informed guidelines, which were rigorously enforced, would make the first question of this paper, "Can better management promote better health for all?" obsolete.},
	author = {Alexander,LM and Wheeler,D. and Heaven,A. and Nath,B. and Robinson,J. P.}
}

@article{Alexander:1992kq,
	ty = {JOUR},
	pages = {340--344},
	date = {2010-12-23 11:37:42 -0600},
	title = {Symptomatology of children in contact with sea water contaminated with sewage},
	keywords = {sea water; anorexia; article; child; coliform bacterium; diarrhea; enterovirus; fatigue; fever; health hazard; human; major clinical study; priority journal; pruritus; salmonella; sea pollution; sewage; streptococcus; symptomatology; united kingdom; vomiting; water sampling; Acute Disease; England; Female; Health Status; Health Surveys; Male; Prevalence; Risk Factors; Seawater; Support, Non-U.S. Gov't; Water Microbiology; Water Pollution},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0026739004&partnerID=40},
	number = {4},
	isbn = {0143005X},
	address = {Affiliation: Environ. Epidemiology Research Unit, Lancaster University, Lancaster LA1 4YB, United Kingdom; Correspondence Address: Alexander, L.M.; Environ. Epidemiology Research Unit; Lancaster University Lancaster LA1 4YB, United Kingdom},
	m = {Journal Article},
	annote = {Study objective-The aim was to determine whether or not there was a measurable risk of ill health associated with contact with sea water for children between the ages of 6 and 11 years old. Design and Setting-This was a prospective survey carried out on Blackpool beach. Parents of children between the ages of 6 and 11 years were interviewed over a seven week period during July, August, and September, 1990. Respondents were followed up 10-14 d after the original interview by either telephone or post. Water samples were collected on each day of the survey. Participants-939 interviews with parents or guardians were completed on the beach; 857 (919{\%}) of these persons agreed to a follow up interview. The results of this study are based on 703 cases of matched data, of good quality, collected for each child on the beach and during a follow up interview. Main results-Non-compliance with the European Community microbiological imperative standards for recreational waters at Blackpool Tower and South Pier sampling sites, respectively, ranged between: 6{\%} and 7{\%} for total coliforms; 13{\%} and 25{\%} for faecal coliforms; 69{\%} and 80{\%} for faecal streptococci (Guide standard); 50{\%} and 67{\%} for salmonellae and 73{\%} and 88{\%} for enteroviruses. There was a significant overall increase in the mean number of symptoms reported for each child (p < 0.001). However, the prevalence of certain symptoms increased significantly only in those children who had been in contact with the water on the day of the beach interview. These symptoms included vomiting (p < 0.0009), diarrhoea (p<0.0001), itchy skin (p<0.0009), fever (p < 0.0013), lack of energy (p < 0.0007), and loss of appetite (p<0.0227). None of the other variables investigated could account for the significant increase in the reported symptom experience of those children in contact with sea water contaminated with sewage. Conclusions-children who come into contact with contaminated sea water are likely to develop symptoms as a result.},
	volume = {46},
	author = {Alexander,L. M. and Heaven,A. and Tennant,A. and Morris,R.},
	journal = {J.Epidemiol.Community Health},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0026739004&partnerID=40},
	year = {1992}
}

@article{Alig:1974tj,
