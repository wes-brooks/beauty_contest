@article{Walkley:2008zb,
	author = {Walkley,N. and Moore,D. and McGee,C. and Kacian,D. and Weeks,I.},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/walkley_2008_investigation_transcription-mediated_amplification_rapid_test_enterococci.pdf},
	year = {2008},
	date = {2010-12-23 11:37:44 -0600},
	journal = {J.Environ.Monit.},
	isbn = {14640325},
	number = {9},
	m = {Journal Article},
	pages = {1064--1068},
	title = {Investigation of transcription-mediated amplification as a rapid test method for Enterococci in recreational water},
	ty = {JOUR},
	address = {Affiliation: Molecular Light Technology Research Ltd., 5 Chiltern Close, Cardiff Industrial Park, CF14 5DL, United Kingdom; Affiliation: Orange County Public Health Laboratories, 700 Shellmaker Road, Newport Beach, CA 92660, United States; Affil(TRUNCATED)},
	keywords = {recreational water; unclassified drug; water; article; bacterium detection; bacterium examination; colony forming unit; comparative study; economic aspect; Enterococcus; feces microflora; gene amplification; health hazard; laboratory; nonhuman; priority journal; quantitative analysis; United States; water contamination; water sampling; Recreation; Regression Analysis; Reproducibility of Results; Transcription, Genetic; Water Microbiology},
	annote = {Growth-based methods for detecting faecal contamination of recreational water require at least 24 hours to yield results, which can delay detection, action and remediation, if required. Such delays can put human health at risk while closing beaches inappropriately can cause unnecessary economic losses. There is a requirement for more rapid methods to facilitate the management of pollution events. In this study, undertaken in our respective laboratories, a molecular assay based on transcription-mediated amplification (TMA) technology was compared with established growth-based methods. The assays were used to quantitate enterococcal faecal indicator bacteria (relative to the legislative threshold of 104 colony forming units/100 mL) in 138 water samples collected from 41 different locations, representative of different recreational water types routinely sampled in Southern California. The results demonstrate the efficacy of the TMA assay for the detection of Enterococcus species in recreational water as a rapid alternative to traditional methods. {\copyright}The Royal Society of Chemistry.},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/walkley_2008_investigation_transcription-mediated_amplification_rapid_test_enterococci.pdf},
	volume = {10}
}

@article{Wallis:1989je,
	m = {Journal Article},
	keywords = {Clyde; Coastal; Dispersion; Estuaries/Inlets; Transport Model; Water Quality},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0024499162&partnerID=40&rel=R8.0.0},
	year = {1989},
	title = {Consideration of a one-dimensional transport model of the upper Clyde estuary.},
	date = {2010-12-23 11:37:42 -0600},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0024499162&partnerID=40&rel=R8.0.0},
	publisher = {Cranfield, U.K., BHRA},
	isbn = {0947711759 (ISBN); 9780947711757 (ISBN)},
	annote = {Describes a one-dimensional time-dependent mathematical model for solute transport in estuaries. The model is applied to an 11 km reach of the Clyde estuary in Scotland and has been developed as part of a larger study into the dispersal and survival of indicator bacteria in coastal and estuarine waters. The model consists of a finite element solution to the advection-dispersion equation with decay which interacts with a finite difference solution to the hydrodynamic equations. The hydrodynamic model has been verified using water level data and measurements of flow velocity, and the conservative version of the solute model has been calibrated approximately using salinity measurements. The hydrodynamic model is shown to perform well, but the solute model remains unverified at the time of writing due to a shortage of appropriate salinity data. The values of the dispersion coefficient used in the model, however, are shown to be consistent with values estimated from measurements of the flow structure. Other work has yielded values for the mortality or decay rate appropriate to Escherichia coli bacteria in estuarine waters which, when used with the nonconservative version of the solute transport model, will enable a bacterial concentration model to be calibrated. (A)},
	author = {Wallis,S. G. and Crowther,J. M. and Curran,J. C. and Milne,D. P. and Findlay,J. S.},
	ty = {JOUR}
}

