@ARTICLE{Bushon:2009nk,
  author = {Bushon,R. N. and Brady,A. M. and Likirdopulos,C. A. and Cireddu,J.
	V.},
  title = {Rapid detection of Escherichia coli and enterococci in recreational
	water using an immunomagnetic separation/adenosine triphosphate technique},
  journal = {JOURNAL OF APPLIED MICROBIOLOGY},
  year = {2009},
  volume = {106},
  pages = {432--441},
  number = {2},
  month = {FEB 2009},
  annote = { The aim of this study was to examine a rapid method for detecting
	Escherichia coli and enterococci in recreational water. Water samples
	were assayed for E. coli and enterococci by traditional and immunomagnetic
	separation/adenosine triphosphate (IMS/ATP) methods. Three sample
	treatments were evaluated for the IMS/ATP method: double filtration,
	single filtration, and direct analysis. Pearson's correlation analysis
	showed strong, significant, linear relations between IMS/ATP and
	traditional methods for all sample treatments; strongest linear correlations
	were with the direct analysis (r = 0.62 and 0.77 for E. coli and
	enterococci, respectively). Additionally, simple linear regression
	was used to estimate bacteria concentrations as a function of IMS/ATP
	results. The correct classification of water-quality criteria was
	67% for E. coli and 80% for enterococci. The IMS/ATP method is a
	viable alternative to traditional methods for faecal-indicator bacteria.
	The IMS/ATP method addresses critical public health needs for the
	rapid detection of faecal-indicator contamination and has potential
	for satisfying US legislative mandates requiring methods to detect
	bathing water contamination in 2 h or less. Moreover, IMS/ATP equipment
	is considerably less costly and more portable than that for molecular
	methods, making the method suitable for field applications.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/bushon_2009_rapid_ecoli_enterococci_immunomagnetic_separation_ATP_technique.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:42 -0600},
  isbn = {1364-5072},
  keywords = {ATP detection; enterococci; Escherichia coli; immunomagnetic separation;
	rapid detection; recreational water},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/bushon_2009_rapid_ecoli_enterococci_immunomagnetic_separation_ATP_technique.pdf}
}

@ARTICLE{Bushon:2008jx,
  author = {Bushon,Rebecca N. and Brady,Amie M. G. and Plona,Meg B.},
  title = {Using a rapid method to predict recreational water quality at Cuyahoga
	Valley National Park, Ohio},
  journal = {Park Science: Integrating Research and Resource Management in the
	National Parks},
  year = {2008},
  volume = {25},
  number = {1},
  month = {Summer 2008},
  annote = {Estimating fecal-indicator bacteria levels using a rapid 1-hour method
	in Cuyahoga Valley National Park recreational waters By Rebecca Bushon
	and Meg Plona Abstract (8-17-2005 The Cuyahoga River connects Cuyahoga
	Valley National Park with the largest system of freshwater in the
	world. This river that burned gave international attention to water-quality
	issues and encouraged action through the passage of environmental
	legislation, especially the Clean Water Act. U.S. Geological Survey
	(USGS) scientists, in partnership with the National Park Service
	(NPS), have been working together to collect scientific information
	to help promote clean water and the preservation of aquatic ecosystems.
	Fiscal year 2005 concluded the field season for testing a new, rapid
	method for estimating Escherichia coli (E. coli) levels in the Cuyahoga
	River. The immunomagnetic (IMS) separation/adenosine triphosphate
	(ATP) rapid method, developed by researchers at the University of
	Michigan, uses magnetic beads that are coated with antibodies that
	are specific for E. coli. The bacteria attach to the antibodies that
	are bound to the magnetic beads and then are separated from the rest
	of the water sample with a magnet. Bacteria levels are measured indirectly
	by way of the concentration of an organic molecule, ATP, contained
	within the bacterial cell. Results of the rapid method are demonstrating
	a strong correlation with the standard 24-hour plating method currently
	used for determining bacteria levels. Results of the IMS/ATP rapid
	method could be provided within an hour of collecting the sample,
	giving parks with freshwater-based recreation the ability to alert
	visitors about bacteria levels. If successful, the rapid bacteria
	method used by the USGS would allow park managers to make decisions
	based on current information---not yesterday's information---and
	disseminate the results to the public.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/bushon_2008_rapid_method_predict_recreational_QW_cuyahoga_valley_national_park_ohio.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:42 -0600},
  isbn = {1090-9966},
  m1 = {Magazine Article},
  publisher = {National Park Service, U.S. Department of the Interior},
