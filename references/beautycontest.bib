	number = {3-4},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0022826034&partnerID=40},
	journal = {Rev Argent Microbiol},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0022826034&partnerID=40},
