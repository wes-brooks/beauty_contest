@ARTICLE{Schepers:1980iw,
  author = {Schepers,J. S. and Vavricka,E. J. and Anderson,D. R. and Wittmuss,H.
	D. and Schuman,G. E.},
  title = {Agricultural Runoff During a Drought Period},
  journal = {Journal of the Water Pollution Control Federation Vol 52},
  year = {1980},
  volume = {April},
  address = {No 4, p 711-719},
  address1 = {Science and Education Administration Lincoln, NE},
  annote = {Basic research on agricultural runoff during a dry period is supplemented
	with specific examples from a 2025-ha agricultural water shed. Runoff
	from the Dee Creek watershed in Nebraska was analyzed over a three-year
	period of less than normal precipitation. Runoff amounted to less
	than 2{\%} of the incoming moisture, and sediment losses were less
	than 0.7 metric ton/ha-year. Nutrient losses associated with the
	sediment averaged 74{\%} of the total N losses and 76{\%} of the
	total P losses. Nutrient loss was not consistently related to fertilizer
	application. The results of this study are in line with data in the
	literature which suggest that nutrient discharge is highly dependent
	upon sediment loss. Water quality of the runoff was also determined.
	Over 90{\%} of the samples collected exceeded recommended recreational
	water quality standards for total coliforms and fecal coliforms.
	Base flows exceeded the same standards 90{\%} of the time for total
	coliforms and 45{\%} of the time for fecal coliforms. (Small-FRC)},
