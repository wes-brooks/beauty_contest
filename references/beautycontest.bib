	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/palmer_1993_colilert_marine_total_coliforms_ecoli.pdf},
	title = {Evaluation of Colilert-Marine Water for detection of total coliforms and Escherichia coli in the marine environment},
