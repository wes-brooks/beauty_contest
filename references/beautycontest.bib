	m = {Conference Proceedings},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-27744481313&partnerID=40},
	isbn = {0784407630 (ISBN)},
	ty = {CONF},
	keywords = {Coliform bacteria; Discharge (fluid mechanics); Environmental protection; Oxygen; Rivers; Standards; Wastewater; Water quality; Dissolved oxygen; Optical brighteners monitoring (OBM); Water quality standards; Watersheds},
	pages = {1257--1264},
