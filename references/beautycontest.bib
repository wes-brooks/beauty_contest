	year = {2003},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/jamieson_2003_sources_persistence_fecal_coliform_rural_watershed.pdf},
	booktitle = {Theme issue -- Drinking water: from source to tap.},
	date = {2010-12-23 11:37:43 -0600},
