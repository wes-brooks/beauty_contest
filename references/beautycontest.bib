@ARTICLE{Hernandez-Delgado:1991gc,
  author = {Hernandez-Delgado,E. A. and Sierra,M. L. and Toranzos,G. A.},
  title = {Coliphages as Alternate Indicators of Fecal Contamination in Tropical
	Waters},
  journal = {Environmental Toxicity and Water Quality, Vol.6, No.2},
  year = {1991},
  volume = {7 tab},
  month = {23 ref. SUBE Grant No. 2SO6RR0},
  address = {p 131-143, May 1991. 1 fig},
  annote = {Strong evidence has recently been found against the use of fecal
	coliforms as indicator organisms of fecal contamination in tropical
	waters due to their indigenous nature in pristine waters. There is
	a great need for the development of more rapid, accurate, and low-cost
	techniques for determining bacteriological water quality. Coliphages
	seem to be an excellent alternative. A method has been developed
	to analyze large volumes of pristine water; the method was then compared
	to a commonly used direct method. The new method involves the filtration
	of water through positively charged filters and a virus-elution step.
	The eluent is then mixed with culture medium , a host bacterium,
	and incubated. Pristine tropical rivers, water collected from bromeliads
	(epiphytic vegetation), sewage-contaminated waters, and marine waters,
	and marine waters , were sampled. Concentrations of indicator bacteria
	were higher than recommended levels for recreational waters, including
	bromeliad waters. Indicator levels were higher in bromeliad waters
	than in sewage-contaminated rivers. Phages were isolated only from
	waters being used for recreational purposes and from sites known
	to be contaminated with domestic sewage, but not from pristine or
	bromeliad waters. These results suggest that there is a correlation
	between the presence of coliphages and fecal contamination. This
	further suggests that coliphages may be reliable indicators of fecal
	contamination in the environment. (See also W92-00963) (Author's
	abstract) 35 013953021},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:43 -0600},
  keywords = {Bacterial analysis; Bacteriophage; Bioindicators; Coliphages; Fecal
	coliforms; Tropical regions; Viruses; Water quality; Filtration;
	Laboratory methods; Microbiological studies; Water analysis; Water
	pollution sources},
  m1 = {Journal Article},
  sp = {and INDUNIV Grant Nos. 9020539303 and WRRI 14},
  ty = {JOUR}
}

@ARTICLE{Herrera:2005dq,
  author = {Herrera,A. and Su{\'a}rez,P.},
  title = {Bacterial markers as tools for coastal water environmental quality
	measurement},
  journal = {Interciencia},
  year = {2005},
  volume = {30},
  pages = {171--176},
  number = {3},
  address = {Affiliation: Ciencias Biol{\'o}gicas, Univ. Sim{\'o}n Bol{\'\i}var,
	Venezuela; Affiliation: Lab. de Microbiol. Acu{\'a}tica, Departamento
	de Biol. de Organismos, Univ. Simon Bolivar. S., Estado Miranda.
	Apartado Postal, 89000. Caracas, Venezuela; Corresp(TRUNCATED)},
  annote = {Bacterial groups such as total heterophytes, sulfate reducers and
	nitrate reducers are key organisms in the transformation and recycling
	of nutrients in aquatic environments, while coliforms and enterococci
	are associated to contamination by wastewaters. These latter bacterial
	groups are considered as useful biological markers in the detection
	of seawater quality regarding fecal material, organic material and
	nutrients. The pupose of this study is to identify which bacterial
	markers are more appropriate for measuring water quality and environmental
	health from the microbiological point of view, in two beaches of
	the Venezuelan Caribbean coast. It was found that fecal coliforms
	and enterococci are the most appropriate markers to determine the
	presence of contamination of fecal origin in the body of water. Also,
	the detection and quantification of fecal coliforms in tissues of
	Tivela mactroides is a good indicator of the risk associated with
	the consumption of raw or slightly cooked marine products. Quantification
	of bacterial markers in marine ecosystems is relevant as it allows
	to estimate bacteriological quality and potential risks to public
	health after the use of the water with touristic purposes or the
	harvesting of marine products. Compliance with current regulations
	should be pomoted, as should be the construction of wastewater treatment
	plants. These actions will lead to the improvement of water quality
	and a better utilization of marine resources.},
