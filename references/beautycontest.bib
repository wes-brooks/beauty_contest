@article{Binding:2008oy,
	date = {2010-12-23 11:37:42 -0600},
	pages = {1702--1711},
	title = {Spectral absorption properties of dissolved and particulate matter in Lake Erie},
	address = {Affiliation: National Water Research Institute, Canada Centre for Inland Waters, 867 Lakeshore Road, Burlington, Ont. L7R 4A6, Canada; Correspondence Address: Binding, C.E.; National Water Research Institute; Canada Centre for Inland Waters; 867(TRUNCATED)},
	isbn = {00344257},
	number = {4},
	ty = {JOUR},
	m = {Journal Article},
	journal = {Remote Sensing of Environment},
	keywords = {Absorption; Aquatic colour; Lake Erie; Remote sensing; Information retrieval; Mathematical models; Microorganisms; Optical properties; Satellite observatories; Water quality; Aquatic colors; Optical models; Water quality information; Particles (particulate matter); bleaching; color; dissolved organic matter; error analysis; lake water; parameterization; particulate matter; phytoplankton; quantitative analysis; satellite imagery; Great Lakes {$[$}North America{$]$}; North America},
	annote = {Spectral absorption properties of particulate and dissolved matter were determined for Lake Erie waters in order to investigate the natural variability of the absorption coefficients required as inputs to optical models for converting satellite observations of water colour into water quality information. Particulate absorption measured using the quantitative filter technique yielded absorption spectra containing a fraction that could not be attributed to phytoplankton pigments, living heterotrophs, mineral sediments, or organic detritus but were indicative of additional absorption by a fraction of dissolved organic matter present in colloidal and/or particle-bound form. Erroneously high phytoplankton absorption coefficients measured at short wavelengths using the filter technique suggested that this particle-bound DOM is removed along with phytoplankton pigments during bleaching by sodium hypochlorite and as such is mistakenly incorporated into the phytoplankton absorption signal. Observations suggest that the selective sorption of fractions of DOM onto suspended particles may be responsible for significant variability in the absorption coefficients of particulate and dissolved matter and may be an important contributor to the total spectral absorption signals in Lake Erie waters. This reservoir of coloured organic matter, which to date has not been seriously considered in the optical properties of coastal and inland waters, may produce significant uncertainties in the parameterization of optical models and the interpretation of in situ and remotely sensed aquatic colour signals. Crown Copyright {\copyright}2007.},
	volume = {112},
	author = {Binding,C. E. and Jerome,J. H. and Bukata,R. P. and Booty,W. G.},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-40649106700&partnerID=40&rel=R8.0.0},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/binding_2008_spectral_absorption_dissolved_particulate_lake_erie.pdf},
	year = {2008}
}

@article{Birnbaum:1995tr,
	ty = {JOUR},
	address = {GBA Water Quality Initiative Committee, 19 Edgecome Ave., Toronto, ON M5N 2X1, Canada},
	booktitle = {15. Annual International Symposium of the North American Lake Management Society, Toronto, ON (Canada), 6-11 Nov 1995},
	annote = {The Georgian Bay Association is a voluntary organization of 27 resident associations, representing 5000 families along the eastern and northern shores of Georgian Bay (Lake Huron) and its adjacent lakes and waterbodies. The Georgian Bay Association Recreational Water Quality Initiative is an ongoing scientific investigation into the recreational water quality in southeastern Georgian Bay which has been carried out with researchers from the Dept. of Microbiology of the University of Toronto. The study focusses on the bacterial and pathogenic contamination of recreational water by cottages and boats. Five indicator microorganisms: 1) fecal coliforms, 2) Escherichia coli, 3) fecal streptococcus, 4) enterococci, and 5) Pseudomonas aeruginosa, were measured in the surface water and bottom sediment for ten (10) weeks in accordance with Federal, Provincial and US E.P.A. Guidelines for Recreational Water Quality and Provincial Water Quality Objectives. Accumulation of bacteria in the bottom sediment and subsequent upwelling to the surface water is presented and the correlation of this reintroduction, with wind velocity and direction, is discussed. A correlation between the number of boats with human waste facilities and bacteria and dissolved organic carbon in the surface water is presented and the effect of the water exchange rate (as represented by fetch) is discussed. Some interim conclusions and recommendations are presented. These recommendations include the need for both ongoing sampling, testing and analysis and improved regulations and enforcement for the safe retention and disposal of human waste from both cottages and boats.},
	volume = {11},
	author = {Birnbaum,J. and Bain,T. and Bachmann,R. W. and Jones,J. R. and Peters,R. H. and Soballe,D. M.},
