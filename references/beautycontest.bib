  publisher = {U.S. Geological Survey},
  ty = {RPRT},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/bunnell_2003_linkages_lignite_aquifers_pathogenic_microbes_kidney_disease_louisiana.pdf},
