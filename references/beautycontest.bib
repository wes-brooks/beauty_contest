	isbn = {0013936X},
	address = {Affiliation: Department of Environmental Health Sciences, Environmental Science and Engineering Program, University of California, Los Angeles, CA 90095-1772, United States; Affiliation: Environmental Water Studies, Department of Civil and Envir(TRUNCATED)},
	m = {Journal Article},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-33747480588&partnerID=40&rel=R8.0.0},
	number = {16},
	keywords = {Bacteria; Coastal zones; Costs; Diseases; Epidemiology; Health risks; Mathematical models; Coastal waters; Enterococci; Gastroenteritis; Public health; Water pollution; coastal water; cost-benefit analysis; economic impact; health risk; pollution; swimming behavior; article; cost control; cost of illness; economic aspect; Enterococcus; gastrointestinal symptom; health care cost; human; population density; risk assessment; risk reduction; seashore; United States; water contamination; water quality; Bathing Beaches; California; Costs and Cost Analysis; Environment; Health Care Costs; Humans; Recreation; Swimming; Water Microbiology; Water Pollutants; North America},
	pages = {4851--4858},
	title = {Regional public health cost estimates of contaminated coastal waters: A case study of gastroenteritis at southern California beaches},
	journal = {Environmental Science and Technology},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/given_2006_public_health_cost_estimates_contminated_coastal_waters_california.pdf},
	year = {2006},
	ty = {JOUR},
	date = {2010-12-23 11:37:43 -0600},
	annote = {We present estimates of annual public health impacts, both illnesses and cost of illness, attributable to excess gastrointestinal illnesses caused by swimming in contaminated coastal waters at beaches in southern California. Beach-specific enterococci densities are used as inputs to two epidemiological dose-response models to predict the risk of gastrointestinal illness at 28 beaches spanning 160 km of coastline in Los Angeles and Orange Counties. We use attendance data along with the health cost of gastrointestinal illness to estimate the number of illnesses among swimmers and their likely economic impact. We estimate that between 627,800 and 1,479,200 excess gastrointestinal illnesses occur at beaches in Los Angeles and Orange Counties each year. Using a conservative health cost of gastroenteritis, this corresponds to an annual economic loss of {\$}21 or {\$}51 million depending upon the underlying epidemiological model used (in year 2000 dollars). Results demonstrate that improving coastal water quality could result in a reduction of gastrointestinal illnesses locally and a concurrent savings in expenditures on related health care costs. {\copyright}2006 American Chemical Society.},
	volume = {40},
	author = {Given,S. and Pendleton,L. H. and Boehm,A. B.}
}

