@book{Fujioka:2010kz,
	m = {Book, Whole},
	sp = {vp},
	ty = {BOOK},
	address = {Water Resour. Res. Cent., Univ. Hawaii, Honolulu, HI 96822, USA},
	annote = {Concentrations of fecal indicator bacteria (fecal coliform, E. coli, enterococci) are used by USEPA to determine the hygienic quality of water and to establish water quality standards. USEPA assumes that the sources of these fecal bacteria are feces of human/animals or sewage. Moreover, that there are no environmental sources of these fecal indicator bacteria. If these assumptions are not valid, the application of these fecal indicator bacteria to assess the quality of water is of questionable value. We have determined that unlike the continental USA which is located in the temperate region of the world, tropical island environments (streams, soil) such as Hawaii and Guam contain high concentrations of fecal indicator bacteria in the absence of fecal or sewage contamination. Thus, in Hawaii, the presence of high concentrations of fecal indicator bacteria in streams does not necessarily indicate that the water is contaminated with feces. We have recently documented that these fecal indicator bacteria can be recovered from all of the seven major soil types in Oahu and appear to be multiplying in the soil environment. Thus, these fecal indicator bacteria can be transported from the soil to streams, stormdrains and coastal waters by runoff from rain, as well as from urban and agricultural uses of water. C. perfringens, an alternative fecal indicator, cannot multiply in the environment and is a better indicator to establish water quality standards in Hawaii. In summary, due to inherent differences in the environment between tropical island conditions and temperate continental conditions, water quality standards specific for that environment should be used.},
	author = {Fujioka,R. and Roll,B. and Byappanahalli,M.},
	title = {Fecal bacteria in Hawaii's soil and their impact on water quality standards for tropical islands},
	publisher = {AMERICAN WATER RESOURCES ASSOCIATION},
	keywords = {Article Subject Terms: Escherichia coli; feces; indicators; soil bacteria; tropical regions; water pollution sources; water quality standards; Article Geographic Terms: USA, Hawaii},
	year = {{$[$}nd{$]$}},
	date = {2010-12-23 11:37:42 -0600},
	booktitle = {Water Resources and Environmental Hazards: Emphasis on Hydrologic and Cultural Insight in the Pacific Rim, Honolulu, HI (USA), 25-28 Jun 1995}
}

