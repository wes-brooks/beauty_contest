@ARTICLE{Muller:1999ll,
  author = {M{\"u}ller,H. E.},
  title = {Real and unreal risks of infectious diseases in bathing waters},
  journal = {Gesundheitswesen},
  year = {1999},
  volume = {61},
  pages = {473--476},
  number = {10},
  address = {Affiliation: Alter Rautheimer Weg 16, D-38126 Braunschweig},
  annote = {Life-threatening infectious diseases associated with recreational
	water exposure are possible but occur very rarely. About eighty per
	cent of all infections due to bathing water produced gastroenteritis,
	the remaining were spontaneously healing infections of ear, nose
	and throat as well as eye and respiratory symptoms. They are caused
	by endogenic and exogenic agents. The risk, i.e. the mean frequency
	of such commonplace diseases associated with bathing or swimming
	is already doubled by endogenic infectious agents even without any
	etiological agents in the recreational water. The additional risk
	by exogenic infectious agents increases with the concentration of
	fecal markers. The prescribed limit E coli value of 200 CPU per 100
	ml of the EU guidelines for bathing waters corresponds to a risk
	increased by the factor 7-8. The Bathing Water Committee of the German
	Federal Environmental Agency issued new recommendations prescribing
	severely lowered limit values. For example, the Committee called
	for 100 CPU of E. coli per 100 ml. However, there are economical
	and legal objections as well as doubts regarding the Committee's
	scientific competence. The lower limit values are associated with
	high expenses for communities and bathers without recognisable returns
	in terms of efficiency. Furthermore, they offend against the legal
	principle of equality because newly constructed pools are not different
	from bathing water regulated according to ED guidelines. Finally,
	the Bathing Water Committee holds scientifically incorrect views
	on infectious agents and on the risks in recreational waters. {\copyright}Georg
