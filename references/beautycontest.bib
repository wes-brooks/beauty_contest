@BOOK{Fujioka:2010kz,
  title = {Fecal bacteria in Hawaii's soil and their impact on water quality
	standards for tropical islands},
  publisher = {AMERICAN WATER RESOURCES ASSOCIATION},
  year = {{$[$}nd{$]$}},
  author = {Fujioka,R. and Roll,B. and Byappanahalli,M.},
  address = {950 HERNDON PARKWAY, SUITE 300, HERNDON, VA 22070-5528 (USA)},
  address1 = {Water Resour. Res. Cent., Univ. Hawaii, Honolulu, HI 96822, USA},
  annote = {Concentrations of fecal indicator bacteria (fecal coliform, E. coli,
	enterococci) are used by USEPA to determine the hygienic quality
	of water and to establish water quality standards. USEPA assumes
	that the sources of these fecal bacteria are feces of human/animals
	or sewage. Moreover, that there are no environmental sources of these
	fecal indicator bacteria. If these assumptions are not valid, the
	application of these fecal indicator bacteria to assess the quality
	of water is of questionable value. We have determined that unlike
	the continental USA which is located in the temperate region of the
	world, tropical island environments (streams, soil) such as Hawaii
	and Guam contain high concentrations of fecal indicator bacteria
	in the absence of fecal or sewage contamination. Thus, in Hawaii,
	the presence of high concentrations of fecal indicator bacteria in
	streams does not necessarily indicate that the water is contaminated
	with feces. We have recently documented that these fecal indicator
	bacteria can be recovered from all of the seven major soil types
	in Oahu and appear to be multiplying in the soil environment. Thus,
	these fecal indicator bacteria can be transported from the soil to
	streams, stormdrains and coastal waters by runoff from rain, as well
	as from urban and agricultural uses of water. C. perfringens, an
	alternative fecal indicator, cannot multiply in the environment and
	is a better indicator to establish water quality standards in Hawaii.
	In summary, due to inherent differences in the environment between
	tropical island conditions and temperate continental conditions,
	water quality standards specific for that environment should be used.},
  annote1 = {SO: Proceedings of Water Resources and Environmental Hazards: Emphasis
	on Hydrologic and Cultural Insights in the Pacific Rim. vp.; Summary
	only. RX: 848516 (on Oct 10, 2008)},
  booktitle = {Water Resources and Environmental Hazards: Emphasis on Hydrologic
	and Cultural Insight in the Pacific Rim, Honolulu, HI (USA), 25-28
	Jun 1995},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:43 -0600},
  keywords = {Article Subject Terms: Escherichia coli; feces; indicators; soil bacteria;
	tropical regions; water pollution sources; water quality standards;
	Article Geographic Terms: USA, Hawaii},
  m1 = {Book, Whole},
  sp = {vp},
  ty = {BOOK}
}

@ARTICLE{Fujioka:1999eb,
  author = {Fujioka,R. and Sian-Denton,C. and Borja,M. and Castro,J. and Morphew,K.
	and Stewart-Tull,D. E. S. and Dennis,P. J. and Godfree,A. F.},
  title = {Soil: the environmental source of Escherichia coli and Enterococci
	in Guam's streams},
  journal = {Journal of applied microbiology},
  year = {1999},
  volume = {85},
  pages = {83S--89S},
  number = {Suppl. 1},
  month = {83S-89S.},
  address = {Osney Mead Oxford OX2 0EL UK, {$[$}URL:http://www.blacksci.co.uk{$]$}},
  address1 = {Water Resources Research Center, University of Hawaii, 2540 Dole Street,
	Holmes Hall 283, Honolulu, HA USA},
  annote = {We have previously documented that faecal indicator bacteria (Escherichia
	coli, faecal coliform, enterococci) recommended by the U.S. Environmental
	Protection Agency (USEPA) to establish recreational water quality
	standards are naturally found in high concentrations in the surface
	and subsurface of soils in Hawaii. Rain, the source of all streams
	in Hawaii, washes the soil sources of faecal bacteria into all the
	streams of Hawaii, at concentrations which consistently exceed the
	USEPA recreational water quality standards. The objective of this
	study was to test the hypothesis that faecal bacteria are able to
	establish themselves in the soil environments of tropical islands
	by conducting the same study in Guam, a tropical pacific island with
	warmer temperatures and higher humidity than Hawaii. The same methods
	and study design used in Hawaii was used in Guam. The results of
	the study conducted in Guam revealed that all streams contain consistently
	high concentrations of faecal coliform, E. coli, and enterocci which
	exceeded the old USEPA recreational water quality standard of 200
	faecal coliform/100 ml as well as the new water quality standards
	of 126 E. coli/100 ml or 33 enterococci/100 ml. These same faecal
	indicator bacteria were recovered in high concentrations in surface
	and sub-surface (18-36 cm depth) soil samples in Guam. Limited coastal
	water analysis showed that most coastal marine waters contain low
	concentrations of faecal bacteria but coastal waters impacted by
	stream run-off showed elevated levels of faecal bacteria. The results
	of this study support the hypothesis that environmental conditions
	in the tropical areas of the world can support the growth and establishment
	of populations of faecal bacteria in the soil. Thus, soil becomes
	an environmental, non-faecal source of faecal indicator bacteria.
	These results indicate that USEPA water quality standards may not
	be directly applicable to tropical island environments.},
  annote1 = {RX: 848516 (on Jul 11, 2008)},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/fujioka_1999_soil_env_source_ecoli_enterococci_guam.pdf},
  booktitle = {SFAM Symp. on Aquatic Microbiology, Lancaster (UK), Jul 1998},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:43 -0600},
  isbn = {1364-5072},
  journal1 = {J.Appl.Microbiol.},
  keywords = {Article Subject Terms: Fecal coliforms; Humidity; Microbial contamination;
	Soil microorganisms; Streams; Tropical environments; Water quality;
	Water quality standards; Article Taxonomic Terms: Enterococcus; Escherichia
	coli; Guam},
  m1 = {Journal Article},
  publisher = {Blackwell Science Ltd.},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/fujioka_1999_soil_env_source_ecoli_enterococci_guam.pdf}
}

@ARTICLE{Fujioka:2001ev,
