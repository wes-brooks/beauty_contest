@ARTICLE{Douarche:2009lj,
  author = {Douarche,C. and Buguin,A. and Salman,H. and Libchaber,A.},
  title = {E. coli and Oxygen: A Motility Transition},
  journal = {Physical Review Letters},
  year = {2009},
  volume = {102},
  pages = {198101-1--198101-4},
  number = {19},
  month = {15 MAY},
  annote = { The motility of Escherichia coli is correlated with oxygen concentration.
	We show that oxygen penetrating into an anaerobic sample induces
	the coexistence of two domains of motile and nonmotile bacteria.
	This coexistence generates a bacterial accumulation at the border
	that propagates slowly with a constant velocity. To show that this
	front propagation follows general scaling arguments, we characterize
	the sharp and fast motile to nonmotile transition occurring when
