@ARTICLE{Bellair:1977hi,
  author = {Bellair,J. T. and Parr-Smith,G. A. and Wallis,I. G.},
  title = {Significance of diurnal variations in fecal coliform die-off rates
	in the design of ocean outfalls},
  journal = {J.WATER POLLUT.CONTROL FED.},
  year = {1977},
  volume = {49},
  pages = {2022--2030},
  number = {9},
  address = {Affiliation: Caldwell Connell Engin., Melbourne, Australia; Correspondence
	Address: Bellair, J.T.Australia},
  annote = {An experimental study of the diurnal variation in the rate of fecal
	coliform die off in seawater is described. The results showed that
	T 90 values varied from a maximum of 40 hr during the night to a
	minimum of 1.9 hr during the day. A relationship between T 90 value
	and hourly solar radiation is established. The diurnal variation
	in T 90 value results in a diurnal variation in the frequency distribution
	and geometric mean of fecal coliform densities at beaches close to
	ocean discharges of wastewater. The implications of this diurnal
	variation on water quality and on the design of ocean outfalls are
	discussed.},
