@ARTICLE{Wagenet:1974ur,
  author = {Wagenet,R. J. and Lawrence,C. H.},
  title = {Recreational effects on bacteriological quality of an impounded water
	supply},
  journal = {J.ENVIRON.HEALTH},
  year = {1974},
  volume = {37},
  pages = {16--20},
  number = {1},
  address = {Affiliation: Dept. Water Sci. Engin., Univ. California, Davis, Calif.,
	United States},
  annote = {It was found that recreational use effects, as monitored by fecal
	coliform counts, were significant in relation to those people in
	direct contact with the lake in high density use situations, and
	of very low significance at all times in terms of the quality of
	that water eventually being withdrawn for treatment for subsequent
	municipal usage. These findings support the position that reservoirs
	can be used jointly for water supply and for recreational purposes.
	Such recreational use must, however, be intelligently managed. Primary
	water contact areas, such as marinas and swimming beaches, should
	be closely and intelligently monitored and controlled in order to
	minimize the hazards to both the recreationist and the domestic consumer
	arising from excessive density of use. In addition, these high density
	use areas should be located such that they are well removed from
	those areas of the lake from which water is withdrawn. This practice
	would serve to not only protect the quality of the intake water,
	but would in addition eliminate the need for additional treatment
	precautions. The implementation of these practices would allow multiple
	use programs to create many new recreational sites in areas where
	those recreational sites already in existence are sorely overburdened
	and threatened with destruction through overuse.},
  annote1 = {Cited By (since 1996): 1},
  bdsk-url-1 = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0016260679&partnerID=40},
