@book{Cabelli:1979rj,
	booktitle = {Presented at: Conference on the biological indicators of water quality, Newcastle-upon-Tyne, UK, 12 Sep 1978},
	year = {1979},
	ty = {BOOK},
	date = {2010-12-23 11:37:42 -0600},
	m = {Book, Whole},
	address = {EPA Mar. Field St., West Kingston, RI, USA},
	isbn = {ISBN 0-471-27590-5.},
	annote = {There is a measurable risk of illness as defined at least by gastrointestinal symptomatology associated with swimming in waters impacted by minicipal sewage wastes. Secondly, these risks can be related to one or more faecal indicators. Finally, these risks are both measurable by carefully designed prospective epidemiological studies, and socially and economically important long before aesthetic constraints become operative. These risks are primarily referable to children, suggesting that most individuals are immune by the time they reach adolescence. Furthermore, it would seem that the aetiological agents in question are much more prevalent in sewage than most of the culturable bacterial and viral pathogens or are more resistant to environmental stress or both. In addition, since the gastrointestinal illnesses have an acute onset and a relatively benign, short course with no significant sequellae, the rotaviruses and the parvo-like viruses should be considered as possible aetiological agents.},
	keywords = {Marine; Freshwater},
	author = {Cabelli,V. J. and ISBN 0-471-27590-5. and James,A. and Evison,L. (eds ).},
	title = {Evaluation of recreational water quality, the EPA approach}
}

@article{Cabelli:1979lb,
	date = {2010-12-23 11:37:42 -0600},
	keywords = {public health; microbial contamination; recreational waters; ANW, New York; coastal waters; beaches; pollution; USA; New York; recreation sites; indicator organisms; disease resistance; gastrointestinal tract diseases; marine environments; association; respiratory tract diseases; symptoms; man; children; gastrointestinal tract; respiratory tract; infection; water-borne diseases; sewage; water; wastes; marine pollution; pollution effects; sewage pollution; Marine},
	address = {Marine Field Stn., Health Effects Res. Lab.-Cin., US Environ. Prot. Agency, Liberty Lane, West Kingston, RI 02892, USA},
	m = {Journal Article},
	annote = {Findings are described from the second year of an epidemiological-microbiological study conducted at New York City beaches as part of the US Environmental Protection Agency program to develop health effects-recreational water quality criteria. Symptomatology rates among swimmers (defined as immersion of the head in the water) relative to non-swimming but beach-going controls at a 'barely acceptable' (BA) beach and a 'relatively unpolluted' (RU) beach were examined. Measurements were made for a number of potential water quality indicators. The symptom rates, categorized as gastrointestinal (GI), respiratory, 'other', and 'disabling' (stayed home, stayed in bed, consulted a physician), were higher among swimmers than nonswimmers. As in the pretest conducted the previous year, the rate of GI symptoms was significantly higher among swimmers relative to nonswimmers at the RA but not the RU beach. Children, Hispanic Americans, and the low-middle socioeconomic groups were identified as the most susceptible portions of the population.},
	author = {Cabelli,V. J. and Dufour,A. P. and Levin,M. A. and McCabe,L. J. and Haberman,P. W. and Jensen,L. D.},
	month = {(1979)},
	journal = {Am.J.Public Health, 69(7), 690-696},
	ty = {JOUR},
	title = {Relationship of microbial indicators to health effects at marine bathing beaches},
	year = {1979}
}

@article{Cabelli:1983vx,
	keywords = {Recreation; Indicator organisms; Water quality; Risk analysis; Risks; Human diseases; Public health; Fecal coliforms; Coliforms; Microorganisms; Enterococcus; Swimming; Marine environment},
	annote = {The derivation of guidelines and standards based on acceptable risk for recreational water quality is explained. The two microbial guidelines or standards most commonly used in the U.S. are the total coliform limit of 1000/100 mL of water and the fecal coliform limit of 200/100 mL. The microbial guideline states that fecal coliforms should be used as the indicator organism for evaluating the microbiological suitability of recreational waters. Coliforms were rejected as the indicator of choice for evaluating sanitary conditions because of their variable correlation with fecal contamination. Ideally, recreational water quality indicators are microorganisms or chemicals whose densities or concentrations in the water can be quantitatively related to swimming-associated health hazards. One problem with the existing microbial guidelines for direct contact recreational waters is that they are not amenable to, or developed in the context of, risk analysis. They were derived from a barely detectable health effect rather than from exposure-response data. Therefore they do not allow for deliberate decisions by local, state, or federal officials about incremental decreases in the health risks involved. Rather, decision making is limited to acceptance or rejection of the specific limits. In a three year study of New York City beaches performed to determine which were the best indicators, the superiority of enterococci versus fecal coliforms was seen from the regression lines displayed on graphs of linear relationships between swimming-associated rates for gastrointestinal symptoms and mean enterococcus and fecal coliform densities in the water. Not only is this criterion more reliable because of the greater credibility of the information obtained, but also the consequences of the illness are more amenable to economic analysis. (Baker-IVI)},
	address = {No. 10, p 1306-1314},
	volume = {October},
	author = {Cabelli,V. J. and Dufour,A. P. and McCabe,L. J. and Levin,M. A.},
	year = {1983},
	date = {2010-12-23 11:37:42 -0600},
	sp = {37 Ref.},
	ty = {JOUR},
	journal = {Journal of the Water Pollution Control Federation Vol.55},
	title = {A Marine Recreational Water Quality Criterion Consistent with Indicator Concepts and Risk Analysis},
	m = {Journal Article}
}

