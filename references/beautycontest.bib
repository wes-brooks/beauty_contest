	keywords = {El Ni{\~n}o-Southern Oscillation; Escherichia coli; Recreational waters; Water quality prediction; dissolved oxygen; water; aquatic environment; Argentina; article; bacteriology; conductance; Cyanobacterium; El Nino; limnology; multiple regression; nonhuman; oscillation; pH measurement; photosynthesis; risk assessment; sanitation; seashore; temperature; urban area; water quality; water sampling; Bathing Beaches; Cyanobacteria; Fresh Water; Hydrogen-Ion Concentration; Meteorological Factors; Oxygen; Rain; Regression Analysis; Urban Health; Water Microbiology},
	date = {2010-12-23 11:37:43 -0600},
