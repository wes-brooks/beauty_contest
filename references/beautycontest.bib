@ARTICLE{Skraber:2009wn,
  author = {Skraber,S. and Ogorzaly,L. and Helmi,K. and Maul,A. and Hoffmann,L.
	and Cauchie,H. -M and Gantzer,C.},
  title = {Occurrence and persistence of enteroviruses, noroviruses and F-specific
