  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:42 -0600},
  m1 = {Report},
  ty = {RPRT},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/2008_los_angeles_river_bacteria_source_identification_study.pdf; https://internal.mdc.usgs.gov/glrwq/RefLinks/2008_los_angeles_river_bacteria_source_identification_study_APPENDICES.pdf}
