	month = {, Sep},
	m = {Book, Whole},
	keywords = {Article Subject Terms: Algal blooms; American Geophysical Union; Coastal zone management; Eutrophication; Hydrology; Meteorological data; Microbiology; Pathogens; Phytoplankton; Prediction; Public health; Risks; Water quality; Article Geographic Terms: USA},
