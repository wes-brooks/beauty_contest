	year = {1995},
	date = {2010-12-23 11:37:42 -0600},
	pages = {118--119},
	title = {Benefits for all from Georgian Bay Association's (GBA's) 1994 Recreational Water Quality Initiative},
	keywords = {Article Subject Terms: benefits; boating; correlation analysis; domestic wastes; feces; fetch; freshwater pollution; lakes; legislation; microbial contamination; pathogenic bacteria; pollution control; pollution legislation; public concern; public participation; recreation; recreation areas; recreational waters; waste disposal; water pollution sources; water quality; water quality control; water quality standards; Article Geographic Terms: Canada, Ontario, Huron L., Georgian Bay; Canada, Georgian Bay; Georgian Bay Association; benefits; public involvement; Freshwater},
