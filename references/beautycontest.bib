@ARTICLE{Lee:1973af,
  author = {Lee,T. N. and McGuire,J. B.},
  title = {An analysis of marine waste disposal in southeast Florida's coastal
	waters},
  journal = {PERGAMON PRESS},
  year = {1973},
  pages = {865--878},
  address = {Affiliation: Dept. Phys. Oceanol., Univ. Miami, Fla., United States},
  annote = {Ocean outfalls along the southeast Florida coast do not discharge
	into the Gulf Stream, but rather into a narrow strip of coastal water
	which is directly under the influence of the Florida Current. Due
	to lateral meanders of this current and production of spin off eddies,
	outfalls located at the 90 foot isobath will discharge into either
	the coastal water, the western edge of the Florida Current or the
	western portion of a cyclonic eddy. The current produced by the complicated
	interplay of the Florida Current with the eastern edge of the Continental
	Shelf will be predominantly north or south, but a sizeable westerly
	component often occurs. The rate of recurrence of eddies is such
	that the residence time of coastal water is of the order of one week.
	When sewage is discharged from an ocean outfall, the buoyancy of
	the less dense sewage effluent causes it to rise to the surface as
	a 'boil'. The sewage will then follow the prevailing current and
	spread out in a 'plume'. The sewage will dilute with sea water in
	the rising boil to about 100:1 of its original concentration. The
	downstream dilution is rather slow providing an additional dilution
	approximately 10:1 at 1 nautical mile from the outfall. The untreated
	sewage released from the Pompano Beach outfall will produce a very
	large area of surface water which is polluted when judged by the
	Florida state bacterial standard for body contact. In general, this
	area will be several miles long and several hundred feet wide with
	the major dimension being oriented along the direction of the prevailing
	current. A high degree of treatment (99{\%} or above) for bacterial
	removal is necessary for an outfall such as the one at Pompano Beach
	if one is to provide a high degree of confidence that immediate near
	shore waters are safe for bathing in the worst possible current conditions.
	A higher bacterial discharge rate through the outfall will require
	better sewage treatment to meet the same standard.},
  bdsk-url-1 = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0015703619&partnerID=40},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:44 -0600},
  keywords = {environmental health},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0015703619&partnerID=40}
}

