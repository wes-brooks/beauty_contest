	year = {2004},
	date = {2010-12-23 11:37:42 -0600},
	m = {Report},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/schneider_2004_water-resources-related_info_sewerage_district_planning_area_milwaukee_wisconsin_1970-2002.djvu},
	booktitle = {Water-Resources Investigations Report}
}

