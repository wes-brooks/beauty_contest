@ARTICLE{Sessitsch:2001yi,
  author = {Sessitsch,A. and Weilharter,A. and Gerzabek,M. H. and Kirchmann,H.
	and Kandeler,E.},
  title = {Microbial Population Structures in Soil Particle Size Fractions of
	a Long-Term Fertilizer Field Experiment},
  journal = {APPLIED AND ENVIRONMENTAL MICROBIOLOGY},
  year = {2001},
  volume = {67},
  pages = {4215--4224},
  number = {9},
  month = {Sept.},
  annote = {Soil structure depends on the association between mineral soil particles
	(sand, silt, and clay) and organic matter, in which aggregates of
	different size and stability are formed. Although the chemistry of
	organic materials, total microbial biomass, and different enzyme
	activities in different soil particle size fractions have been well
	studied, little information is available on the structure of microbial
	populations in microhabitats. In this study, topsoil samples of different
	fertilizer treatments of a long-term field experiment were analyzed.
	Size fractions of 200 to 63 mm (fine sand fraction), 63 to 2 mm (silt
	fraction), and 2 to 0.1 mm (clay fraction) were obtained by a combination
	of low-energy sonication, wet sieving, and repeated centrifugation.
	Terminal restriction fragment length polymorphism analysis and cloning
	and sequencing of 16S rRNA genes were used to compare bacterial community
	structures in different particle size fractions. The microbial community
	structure was significantly affected by particle size, yielding higher
	diversity of microbes in small size fractions than in coarse size
	fractions. The higher biomass previously found in silt and clay fractions
	could be attributed to higher diversity rather than to better colonization
	of particular species. Low nutrient availability, protozoan grazing,
	and competition with fungal organisms may have been responsible for
	reduced diversities in larger size fractions. Furthermore, larger
	particle sizes were dominated by a-Proteobacteria, whereas high abundance
	and diversity of bacteria belonging to the Holophaga/Acidobacterium
	division were found in smaller size fractions. Although very contrasting
	organic amendments (green manure, animal manure, sewage sludge, and
	peat) were examined, our results demonstrated that the bacterial
	community structure was affected to a greater extent by the particle
	size fraction than by the kind of fertilizer applied. Therefore,
	our results demonstrate specific microbe-particle associations that
	are affected to only a small extent by external factors.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/sessitsch_2001_microbial_population_structures_soil_particle_size_fertilizer_field_experiment.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:44 -0600},
  isbn = {0099-2240/01/{\$}04.0010},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/sessitsch_2001_microbial_population_structures_soil_particle_size_fertilizer_field_experiment.pdf}
}

