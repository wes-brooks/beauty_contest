  journal1 = {Environ. Sci. Technol.},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/byappanahalli_2008_enterococcal_surface_protein_esp_gene_unreliable_indicator.pdf}
}

@ARTICLE{Byappanahalli:2009ed,
