	USA, Maryland, Anne Arundel County},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0030319329&partnerID=40&rel=R8.0.0}
