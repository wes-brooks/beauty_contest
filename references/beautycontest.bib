  date-modified = {2010-12-23 11:37:43 -0600},
  isbn = {0273-1223},
  journal1 = {Water.science and technology : a.journal of the International Association
	on Water.Pollution Research},
  keywords = {Bacillus; Clostridium perfringens; Enterobacteriaceae; Environmental
	Monitoring; Escherichia coli; Feces: microbiology; Humans; Particle
	Size; Population Dynamics; Rain; Recreation; Soil Pollutants: analysis;
	Spores, Bacterial; Water Microbiology; Water Movements; Water Pollutants:
