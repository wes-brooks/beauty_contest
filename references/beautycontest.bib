	keywords = {Contaminants; Database; Human dimensions; Southeast; Tidal creek; Water quality},
	author = {White,D. L. and Wolf,D. and Porter,D. E. and Sanger,D. M. and Riekerk,G. H. M. and DiDonato,G. and Holland,A. F. and Dabney,D.},
	ty = {JOUR},
	pages = {1--9},
	title = {Development of a data management framework in support of southeastern tidal creek research},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/white_2008_development_data_management_framework_support_southeastern_tidal_creek.pdf},
	year = {2008},
	m = {Journal Article},
