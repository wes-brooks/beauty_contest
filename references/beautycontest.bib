@article{Jabar-Al-Mossawi:1983wb,
	ty = {JOUR},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0021057204&partnerID=40},
	address = {Affiliation: Environ. Sci. Dep., Kuwait Inst. Sci. Res., Safat, Kuwait; Correspondence Address: Al-Mossawi, M.JabarKuwait},
	keywords = {MARINE BIOLOGY; SEAWATER; WATER POLLUTION; antibiotic resistance; clam; coliform bacterium; controlled study; invertebrate; methodology; nonhuman; salmonella; sea pollution; water pollution indicator; bacteria; Circenita callipyga; coliform; fecal},
	annote = {The coastal waters of Kuwait are widely used for bathing during the long summer season. In order to assess the water quality along the beaches, a new approach was developed. It involves the use of clam, Circenita callipyga, as a bioindicator for seawater pollution by fecal coliform bacteria (FC), antibiotic resistant fecal coliform (ABR-FC) and Salmonella spp. These organisms were detected in clam and water samples and the efficiency of clam in concentrating Salmonella was confirmed. The number of FC and ABR-FC in clam and water samples were compared and a higher number was detected in clams than in water. Five isolates of Salmonella from clams were also multidrug resistant. The data suggests that clam can be used as a bioindicator of seawater pollution and is preferable to direct testing of water. The resistance to antibiotics in E. coli isolated from water and clams collected at the same time and site was of similar pattern and was determined by R-plasmid.},
	volume = {20},
	author = {Jabar Al Mossawi,M. and Kadri,M. and Salem,A. A. and Chugh,T. D.},
	pages = {257--263},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/jabar_al-mossawi_1983_use_clams_bioindicator_fecal_pollution_seawater.pdf},
	title = {The use of clams as bioindicator of fecal pollution in seawater},
	year = {1983},
	date = {2010-12-23 11:37:42 -0600},
