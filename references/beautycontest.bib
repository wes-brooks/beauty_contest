@article{Nevers:2007kt,
	month = {Sep-Oct},
	isbn = {0047-2425},
	address = {Great Lakes Science Center, Lake Michigan Ecological Research Station, 1100 N. Mineral Springs Road, Porter, IN 46304 USA. mnevers{\char64}usgs.gov},
	m = {Journal Article},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/nevers_2007_interaction_influence_two_creeks_nearby_beaches_predictability_mechanisms.pdf},
	number = {5},
	keywords = {Bathing Beaches; Colony Count, Microbial; Environmental Monitoring; Escherichia coli/isolation \& purification; Fresh Water/microbiology; Michigan; Rain},
	pages = {1338--1345},
	title = {Interaction and influence of two creeks on Escherichia coli concentrations of nearby beaches: exploration of predictability and mechanisms},
	journal = {Journal of environmental quality},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/nevers_2007_interaction_influence_two_creeks_nearby_beaches_predictability_mechanisms.pdf},
	year = {2007},
	ty = {JOUR},
	date = {2010-12-23 11:37:44 -0600},
	annote = {The impact of river outfalls on beach water quality depends on numerous interacting factors. The delivery of contaminants by multiple creeks greatly complicates understanding of the source contributions, especially when pollution might originate up- or down-coast of beaches. We studied two beaches along Lake Michigan that are located between two creek outfalls to determine the hydrometeorologic factors influencing near-shore microbiologic water quality and the relative impact of the creeks. The creeks continuously delivered water with high concentrations of Escherichia coli to Lake Michigan, and the direction of transport of these bacteria was affected by current direction. Current direction reversals were associated with elevated E. coli concentrations at Central Avenue beach. Rainfall, barometric pressure, wave height, wave period, and creek specific conductance were significantly related to E. coli concentration at the beaches and were the parameters used in predictive models that best described E. coli variation at the two beaches. Multiple inputs to numerous beaches complicates the analysis and understanding of the relative relationship of sources but affords opportunities for showing how these complex creek inputs might interact to yield collective or individual effects on beach water quality.},
	volume = {36},
	author = {Nevers,M. B. and Whitman,R. L. and Frick,W. E. and Ge,Z.}
}

