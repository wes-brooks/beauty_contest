@article{Auld:2004fe,
	number = {20-22},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/auld_2004_heavy_rainfall_waterborne_disease_outbreaks_walkerton_example.pdf},
	year = {2004},
	ty = {JOUR},
	date = {2010-12-23 11:37:42 -0600},
	m = {Journal Article},
	address = {Affiliation: Sci. Assess. and Integration Branch, Meteorological Service of Canada, Toronto, Ont., Canada; Affiliation: Adaptation and Impacts Res. Group, Meteorological Service of Canada, Toronto, Ont., Canada; Affiliation: Atmospheric Science (TRUNCATED)},
	isbn = {15287394},
	annote = {Recent research indicates that excessive rainfall has been a significant contributor to historical waterborne disease outbreaks. The Meteorological Service of Canada, Environment Canada, provided an analysis and testimony to the Walkerton Inquiry on the excessive rainfall events, including an assessment of the historical significance and expected return periods of the rainfall amounts. While the onset of the majority of the Walkerton, Ontario, Escherichia coli O157:H7 and Campylobacter outbreak occurred several days after a heavy rainfall on May 12, the accumulated 5-d rainfall amounts from 8-12 May were particularly significant. These 5-d accumulations could, on average, only be expected once every 60 yr or more in Walkerton and once every 100 yr or so in the heaviest rainfall area to the south of Walkerton. The significant link between excess rainfall and waterborne disease outbreaks, in conjunction with other multiple risk factors, indicates that meteorological and climatological conditions need to be considered by water managers, public health officials, and private citizens as a significant risk factor for water contamination. A system to identify and project the impacts of such challenging or extreme weather conditions on water supply systems could be developed using a combination of weather/climate monitoring information and weather prediction or quantitative precipitation forecast information. The use of weather monitoring and forecast information or a "wellhead alert system" could alert water system and water supply managers on the potential response of their systems to challenging weather conditions and additional requirements to protect health. Similar approaches have recently been used by beach managers in parts of the United States to predict day-to-day water quality for beach advisories.},
	volume = {67},
	author = {Auld,H. and MacIver,D. and Klaassen,J.},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-4644266545&partnerID=40},
	keywords = {rain; Campylobacter; Canada; climate change; conference paper; disease transmission; environmental monitoring; epidemic; Escherichia coli O157; government; health hazard; meteorology; priority journal; public health; risk factor; water contamination; water quality; water supply; weather; Campylobacter Infections; Communicable Disease Control; Communicable Diseases; Disease Outbreaks; Escherichia coli Infections; Humans; Ontario; Water Microbiology; Escherichia coli},
	journal = {J.Toxicol.Environ.Health Part A},
	pages = {1879--1887},
	title = {Heavy rainfall and waterborne disease outbreaks: The Walkerton example}
}

