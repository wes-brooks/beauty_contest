	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/kay_2007_catchment_microbial_dynamics_emergence_research_agenda.pdf},
	year = {2007},
	date = {2010-12-23 11:37:43 -0600},
	keywords = {catchment microbial dynamics; Clean Water Act; coliform; faecal indicator organisms; modelling; Water Framework Directive},
	number = {1},
	m = {Journal Article},
	pages = {59--76},
	title = {Catchment microbial dynamics: the emergence of a research agenda},
