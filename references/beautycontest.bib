@ARTICLE{Verhoeven:2002iq,
  author = {Verhoeven,R.},
  title = {The structure of the microtrophic system in a development series
	of dune soils},
  journal = {Pedobiologia},
  year = {2002},
  volume = {46},
  pages = {75--89},
  number = {1},
  address = {Affiliation: University of Bremen, Centre for Environmental Research
	and Technology, Leobener Strasse, D-28359 Bremen, Germany; Correspondence
	Address: Verhoeven, R.; University of Bremen; Ctr. for Environ. Res./Technology;
	Leobener Strasse D-28(TRUNCATED)},
  annote = {In coastal dunes the structure of the microtrophic system was investigated
	on the basis of the relationship between testaceans, ciliates, nematodes,
	and rotifers within the microfaunal community. The eight investigated
	sites follow a gradient of soil development on coastal dune sand.
	Abundance of microfauna increased from sandy beach to forest and
	heath sites, dividing the sites into two distinct groups of successional
	stages: the young sites (beach, primary and white dune/grey dune)
	and the more mature sites (old grey dune, shrub, dune heath, and
	forest). The content of organic matter in soil, also influencing
	water conditions and pH, was the main factor with respect to abundance
	of all microfaunal groups. Testaceans were highly dominant in all
	sites except the beach and account for up to 95 {\%} of the total
	microfauna abundance. While ciliates were more abundant than nematodes
	in the young soils, the latter became more important in the soils
	with an accumulation of organic matter, where also rotifers occurred
	in higher numbers. The investigated microfaunal groups represent
	the higher trophic levels of the microtrophic system. The results
	show an increasing complexity of food web interactions and more trophic
	levels in older sites than in early successional stages. The changes
	in the soil properties and the energy source lead to a shift in the
	food web structure from bacterial dominated to detritus dominated
