  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/goodwin_2009_performance_chromagar_seawater_sand_culture_agglutination_molecular.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:43 -0600},
  isbn = {0043-1354/{\$}},
  keywords = {Staphylococcus aureus; MRSA; Recreational water quality; CHROMagar;
	clfA; mecA},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/goodwin_2009_performance_chromagar_seawater_sand_culture_agglutination_molecular.pdf}
}

