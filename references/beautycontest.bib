@ARTICLE{Elmir:2007xr,
  author = {Elmir,Samir M. and Wright,Mary E. and Abdelzaher,Amir and Solo-Gabriele,Helena
	M. and Fleming,Lora E. and Miller,Gary and Rybolowik,Michael and
	Shih,Meng-Ta Peter and Pillai,Segaran P. and Cooper,Jennifer A. and
	Quaye,Elesi A.},
  title = {Quantitative evaluation of bacteria released by bathers in a marine
