@article{Kashefipour:2002en,
	journal = {Water Res.},
	keywords = {Bathing waters; Decay rates; Estuaries; Faecal coliform; Hydroinformatics; Water quality; Coliform bacteria; Sewage treatment plants; Water tanks; Coastal waters; Hydrology; bathing water; compliance; estuary; fecal coliform; article; bath; coliform bacterium; controlled study; feces microflora; hydrodynamics; model; nonhuman; priority journal; river; sediment transport; waste water management; water pollution; weather; Data Collection; England; Environment; Models, Theoretical; Rain; Seasons; Time Factors; Water Microbiology; Water Purification; Water Supply; Wind; Bacteria (microorganisms)},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0036222150&partnerID=40&rel=R8.0.0},
	m = {Journal Article},
	number = {7},
	ty = {JOUR},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/kashefipour_2002_hydro-environmental_modelling_bathing_water_compliance_estaurine_basin.pdf},
	year = {2002},
	date = {2010-12-23 11:37:43 -0600},
	address = {Affiliation: Environmental Water Management Research Centre, Cardiff School of Engineering, University of Wales, P.O. Box 925, Cardiff, CF24 0YF, United Kingdom; Correspondence Address: Falconer, R.A.; Environ. Water Management Centre; Cardiff S(TRUNCATED)},
	pages = {1854--1868},
	title = {Hydro-environmental modelling for bathing water compliance of an estuarine basin},
	annote = {In recent years, considerable investment has been committed to sewerage infrastructure and new sewage treatment plants in the catchment surrounding an estuarine basin along the north-west coast of England. Although this capital investment has resulted in a marked reduction in the input of bacterial loads, relatively high counts of faecal indicator organisms are still being encountered in the coastal receiving waters, and the local bathing waters continue to fail on occasions to comply with the European Community (EC) Bathing Water Directive (1976) mandatory standards. Details are given herein of a comprehensive modelling study aimed at quantifying the impact of various bacterial inputs into the estuary and surrounding coastal waters on the bathing water quality. The model domain includes the coastal area and the entire estuary (namely the Ribble) up to the tidal limits of its tributaries. Faecal coliforms have been used as the main water quality indicator organisms. The numerical model developed for this study combines a depth integrated two-dimensional coastal model and a cross-sectionally integrated one-dimensional river model, and is capable of predicting water surface elevations, velocity fields and faecal coliform concentration distributions across the entire model domain. The hydrodynamic model was calibrated using water level and velocity measurements from three surveys and then validated against measured data from three other surveys. In order to predict the faecal coliform concentration distributions, variable faecal coliform decay rates were used, i.e. different values of decay rates were applied to the coastal and riverine waters, for day- and nighttime, and for wet and dry weather conditions. The maximum and minimum decay rates used were 2.32/day and 0.71/day for the dry and wet weather surveys, respectively. The model was then applied to (i) assess the impact of previous discharge strategies and investigate the effectiveness of future capital investment works and (ii) predict the impact of a range of strategic options, including: the effects of adding UV treatment, constructing storm water storage tanks and incorporating various combined sewer overflow (CSO) discharge scenarios for different weather conditions. Copyright {\copyright}2002 Elsevier Science Ltd.},
	isbn = {00431354},
	volume = {36},
	author = {Kashefipour,S. M. and Lin,B. and Harris,E. and Falconer,R. A.}
}

