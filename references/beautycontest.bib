@article{Helmer:1991cl,
	number = {2},
	month = {1 April 1990 through 6 April 1990},
	m = {Journal Article},
	booktitle = {Proceedings of the IAWPRC International Symposium},
	address = {Affiliation: Division of Environmental Health, World Health Organization, CH-1211 Geneva 27, Switzerland; Correspondence Address: Helmer, R.; World Health Organization Geneva, Switzerland},
	annote = {Over the past decade, the World Health Organization has developed a series of microbiological guidelines covering different aspects of the aquatic environment. Transmission pathways of human pathogens from their faecal origin back to man via drinking-water, bathing waters, shellfish and sewage-irrigated crops were investigated and epidemiology-based health criteria established. The total and faecal coliform guideline values for drinking-water are generally complied with in urban water supplies but difficult to meet in rural areas of developing countries. The Mediterranean serves as the major study area for assessing health risks from bathing in coastal waters and from the consumption of shellfish eaten uncooked. Wastewater reuse for crop irrigation is rapidly expanding due to water shortages but poses health risks to farm workers and consumers. New guidelines for control of helminths were recently issued by a WHO Scientific Group. A comparative evaluation of the above guidelines follows.},
