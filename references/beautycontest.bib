@article{Wagenet:1974ur,
	isbn = {00220892},
	m = {Journal Article},
	number = {1},
	keywords = {coliform bacterium; methodology; microorganism; prevention; recreation; water pollution; water supply},
	pages = {16--20},
	title = {Recreational effects on bacteriological quality of an impounded water supply},
	address = {Affiliation: Dept. Water Sci. Engin., Univ. California, Davis, Calif., United States},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0016260679&partnerID=40},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0016260679&partnerID=40},
	year = {1974},
	annote = {It was found that recreational use effects, as monitored by fecal coliform counts, were significant in relation to those people in direct contact with the lake in high density use situations, and of very low significance at all times in terms of the quality of that water eventually being withdrawn for treatment for subsequent municipal usage. These findings support the position that reservoirs can be used jointly for water supply and for recreational purposes. Such recreational use must, however, be intelligently managed. Primary water contact areas, such as marinas and swimming beaches, should be closely and intelligently monitored and controlled in order to minimize the hazards to both the recreationist and the domestic consumer arising from excessive density of use. In addition, these high density use areas should be located such that they are well removed from those areas of the lake from which water is withdrawn. This practice would serve to not only protect the quality of the intake water, but would in addition eliminate the need for additional treatment precautions. The implementation of these practices would allow multiple use programs to create many new recreational sites in areas where those recreational sites already in existence are sorely overburdened and threatened with destruction through overuse.},
	volume = {37},
	date = {2010-12-23 11:37:44 -0600},
	author = {Wagenet,R. J. and Lawrence,C. H.},
	journal = {J.ENVIRON.HEALTH},
	ty = {JOUR}
}

