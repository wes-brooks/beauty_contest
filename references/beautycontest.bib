	keywords = {Coastal zones; Coliform bacteria; Marine biology; Sediments; Sewage sludge; Pathogens; Sedimentology},
	isbn = {01977385},
	pages = {1844--1853},
	title = {Suitable indicator for the presence of sewage sludge in marine sediments}
