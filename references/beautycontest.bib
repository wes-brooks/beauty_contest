  keywords = {Groundwater quality; Pollution; Unconfined aquifers; Wastewater; effluent
	treatment; geotechnical aspect; sewage disposal; sewage treatment;
	effluent; geotechnical property; groundwater pollution; waste management},
  m1 = {Journal Article},
