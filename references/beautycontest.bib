@book{Francy:1999jn,
	keywords = {Article Subject Terms: Bacteria (Enterobacteriaceae) (Escherichia); Bathing waters; Beaches; Biological pollutants; Conferences; Environmental factors; Escherichia Coli; Lakes; Microbial contamination; Microbiological Studies; Microbiological analysis; Outfalls; Pathogenic bacteria; Pollution (Microbiological); Pollution effects; Public health; Public-health; Rainfall; Recreation; Recreation and amenity; Recreation areas; Recreational waters; Sediment pollution; Statistical Analysis; Swimming; Turbidity; Velocity; Waste water; Wastewater Disposal; Wastewater Pollution; Water Quality; Water Sampling; Water quality (Natural waters); Water wells; Wave height; metropolitan areas; overflow; water temperature; Article Taxonomic Terms: Escherichia coli; Article Geographic Terms: North America, Erie L.; North America, Great Lakes; USA, Ohio; USA, Ohio, Cleveland; USA, Ohio, Erie L.; North America, Erie L.; models; Freshwater},
	publisher = {International Association for Great Lakes Research},
	pages = {A--33},
	title = {Factors affecting Escherichia coli concentrations at Lake Erie public bathing beaches},
	ty = {BOOK},
	booktitle = {42. Conference of the International Association for Great Lakes Research, Cleveland, OH (USA), 24-28 May 1999},
	address = {2205 Commonwealth Boulevard Ann Arbor MI 48105 USA, {$[$}URL:http://iaglr.org/{$]$}},
	year = {1999},
	m = {Book, Whole},
	date = {2010-12-23 11:37:43 -0600},
	annote = {The environmental and water-quality factors that affect concentrations of Escherichia coli (E. coli) in water and sediment were investigated at three public bathing beaches in the Cleveland, Ohio metropolitan area. This study was done to aid in the determination of safe recreational use and to help water-resource managers assess more quickly and accurately the degradation of recreational water quality. Water and lake-bottom sediments were collected and ancillary environmental data were compiled for 41 days from May through September 1997. Turbidity, antecedent rainfall, volumes of wastewater-treatment plant overflows and metered outfalls, a resuspension index, and wave heights were found to be statistically related to E. coli concentrations; however, wind speed, wind direction, water temperature, and the presence of swimmers were shown to be statistically unrelated. Multiple linear regression (MLR) was used to develop a model to predict E. coli concentrations, and thus the recreational water quality, at the three beaches. The chosen MLR model used weighted categorical rainfall, turbidity, and wave height to predict E. coli concentrations. This model accounted for 58 percent of the variability in E. coli concentrations and for 1997, predicted the recreational water quality as well as, and in some cases better than the current method.},
	author = {Francy,DS and Darner,RA}
}

@techreport{Francy:1999dy,
	volume = {98-4241},
	author = {Francy,D. and Darner,R. and Geological Survey, Columbus, OH. Water Resources Div. and Northeast Ohio Regional Sewer District, Cleveland, OH. and Ohio Water Development Authority, Columbus. and Cuyahoga County Board of Health, Cleveland, OH. and Cuyahoga River Community Planning Organization, Cleveland, OH},
	booktitle = {Report Number: USGS/WRI-98-4241},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/francy_1999_factors_affecting_ecoli_concentrations_lake_erie_beaches.djvu},
	keywords = {Article Subject Terms: Bacteria (Enterobacteriaceae) (Escherichia); Beach; Beaches; Decomposition; Degradation; Escherichia Coli; Lakes; Model Studies; Overflow; Overflowing; Plants; Plants (see also Aquatic macrophytes); Precipitation; Prices; Public Health; Rainfall; Sediment; Sediments; Urban Areas; Varieties; Water Quality; Wave Height; Weather; Article Geographic Terms: USA, Erie L.; USA, Ohio, Cleveland; USA, Erie L.},
	title = {Factors Affecting 'Escherichia coli' Concentrations at Lake Erie Public Bathing Beaches},
	sp = {54 p.},
	ty = {RPRT},
	m = {Report},
	bdsk = {https://internal.mdc.usgs.gov/glrwq/RefLinks/francy_1999_factors_affecting_ecoli_concentrations_lake_erie_beaches.djvu},
	year = {1999},
	month = {Water-Resources Investigations Report},
	date = {2010-12-23 11:37:42 -0600},
	annote = {This report describes field studies done throughout the recreational season of 1997 (May through September) at three public bathing beaches in the Cleveland, Ohio, metropolitan area. Concentrations of E. coli were determined in water and lake-bottom sediments collected during a variety of environmental conditions--during dry, calm weather; before, during and after rainfall; for various increased wave heights; and before and after heavy recreational use. The concentrations of E. coli in water and lake-bottom sediments were plotted as a function of time along with wave heights, occurrence of wastewater-treatment plant overflows, and rainfall amounts. These plots were examined qualitatively to determine if these factors affected E. coli concentrations in water and sediment. This report provides water-resource managers with information on the water-quality and environmental factors that affect fecal-indicator concentrations at three Lake Erie beaches. It also provides evidence that predictive models may be developed to help water-resource managers more quickly and accurately assess the degradation of recreational water quality to protect the public health.}
}

