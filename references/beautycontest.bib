	and Andrews,R. C.},
  title = {Chlorine Dioxide Inactivation of Cryptosporidium parvum Oocysts and
	Bacterial Spore Indicators},
  journal = {Applied and Environmental Microbiology},
  year = {2001},
  volume = {67},
  pages = {2993--3001},
