  address = {Affiliation: Klinisk Mikrobiologisk Afdeling, Statens Seruminstitut,
	DK-2300 K{\o}benhavn S; Correspondence Address: Klinisk Mikrobiologisk
	Afdeling; Statens Seruminstitut DK-2300 K{\o}benhavn S},
  annote = {The clinical manifestations and epidemiological data of 11 patients
