	journal = {Antonie Van Leeuwenhoek Int.J.Gen.Mol.Microbiol.},
	keywords = {Diffusion chambers; Fecal indicators; Recreational waters; Salmonella serotypes; Salmonella survival; Water pollution; coastal waters; estuarine environment; fecal coliform; water quality; article; bacterium isolation; coliform bacterium; diffusion chamber; Enterococcus faecalis; estuary; feces; incidence; nonhuman; Portugal; priority journal; Salmonella; Salmonella enteritidis; salmonella infantis; Salmonella typhimurium; Salmonella virchow; serotype; Streptococcus; water analysis; water contamination; water sampling; Bathing Beaches; Colony Count, Microbial; Diffusion Chambers, Culture; Reproducibility of Results; Seawater; Specimen Handling; Water Microbiology; Bacteria (microorganisms); Enteritidis; Escherichia coli; Posibacteria; Salmonella enterica subsp. enterica serovar Infantis; Salmonella enterica subsp. enterica serovar Virchow; Streptococcus infantis; Typhimurium},
