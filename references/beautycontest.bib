@ARTICLE{Byappanahalli:2003xj,
  author = {Byappanahalli,M. N. and Shively,D. A. and Nevers,M. B. and Sadowsky,M.
	J. and Whitman,R. L.},
  title = {Growth and survival of Escherichia coli and enterococci populations
	in the macro-alga Cladophora (Chlorophyta)},
  journal = {FEMS Microbiology Ecology},
  year = {2003},
  volume = {46},
  pages = {203--211},
  annote = {The macro-alga Cladophora glomerata is found in streams and lakes
	worldwide. High concentrations of Escherichia coli and enterococci
	have been reported in Cladophora along the Lake Michigan shore. The
	objective of this study was to determine if Cladophora supported
	growth of these indicator bacteria. Algal leachate readily supported
	in vitro multiplication of E. coli and enterococci, suggesting that
	leachates contain necessary growth-promoting substances. Growth was
	directly related to the concentration of algal leachate. E. coli
	survived for over 6 months in dried Cladophora stored at 4 degC;
	residual E. coli grew after mat rehydration, reaching a carrying
	capacity of 8 log CFU g-1 in 48 h. Results of this study also show
	that the E. coli strains associated with Cladophora are highly related;
	in most instances they are genetically different from each other,
	suggesting that the relationship between E. coli and Cladophora may
	be casual. These findings indicate that Cladophora provides a suitable
	environment for indicator bacteria to persist for extended periods
	and to grow under natural conditions.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/byappanahalli_2003_growth_survival_ecoli_enterococci_populations_cladophora.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:42 -0600},
  isbn = {0168-6496/03},
  keywords = {Cladophora; Environmental condition; Escherichia coli; Enterococci;
	Great Lakes},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/byappanahalli_2003_growth_survival_ecoli_enterococci_populations_cladophora.pdf}
}

