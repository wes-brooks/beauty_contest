  volume = {87},
  number = {36; Suppl.},
  address = {2000 Florida Ave., N.W. Washington DC 20009 USA, {$[$}URL:http://www.agu.org/pubs/agu{\_}joureos.html{$]$};
	Transactions, American Geophysical Union},
  month = {, Sep},
  address1 = {Water Resources Research Center, University of Hawaii, 2540 Dole Street,
	Honolulu, HI 96822 United States, {$[$}mailto:walterb{\char64}hawaii.edu{$]$}},
  annote = {All streams in Hawaii contain elevated levels of Escherichia coli
	and enterorocci that exceed the United States Environmental Protection
	Agency recreational water quality standards. The primary source of
	these fecal indicator bacteria in streams is environmental (soil)
	and not sewage. Thus, an alternative method is required to determine
	when streams and coastal waters receiving stream runoff are contaminated
	with sewage. Results of a recent sewage source tracking methods study
	showed that molecular methods used to detect Bacteroides were reliable
	markers of human and animal feces. The objective of this study was
	to evaluate the reliability of using molecular detection methods
	for fecal Bacteroides 16S rRNA markers to determine when streams
	and coastal waters in Hawaii are contaminated with human sewage.
	The specificity and sensitivity of both standard PCR (SPCR) and quantitative
	PCR (QPCR) assays using primers for general and for human-specific
	Bacteroides markers were evaluated. When SPCR methods were used,
	the human specific markers were more specific to sewage than general
	markers but were less sensitive. However, the analytical sensitivity
	of the QPCR assays for detecting general and human- specific Bacteroides
	markers were shown to be superior to the comparable SPCR assay, which
	requires a reamplification step in order to provide visible PCR products.
	The general Bacteroides marker was detected in ambient and polluted
	water samples while the human-specific marker was primarily detected
	in samples known to be contaminated with sewage such as sewage ocean
	outfalls and sewage spill sites. In conclusion, the results of this
	study showed that PCR- based detection methods for human-specific
	Bacteroides markers can be used to determine when stream and coastal
	water sites are contaminated with human sewage. This kind of information
	can not be obtained when these waters are only monitored for standard
	fecal indicators used to establish water quality standards.},
  annote1 = {NU: OS23D-04; TR: CS0766278 RX: 848516 (on Jul 11, 2008)},
  booktitle = {2006 Ocean Sciences Meeting, Honolulu, HI (USA), 20-24 Feb 2006},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:42 -0600},
  isbn = {0096-3941},
  keywords = {Article Subject Terms: Biological pollutants; DNA; Nucleotide sequence;
	Pathogenic bacteria; Pollution detection; Public health; Sewage;
	Water pollution; Water quality; Article Taxonomic Terms: Bacteroides;
	Escherichia coli; Article Geographic Terms: ISE, USA, Hawaii; Marine},
  m1 = {Book, Whole},
  sp = {{$[$}np{$]$}. suppl},
  ty = {BOOK}
}

@ARTICLE{Beversdorf:2007qk,
  author = {Beversdorf,L. J. and Bornstein-Forst,S. M. and McLellan,S. L.},
  title = {The potential for beach sand to serve as a reservoir for Escherichia
	coli and the physical influences on cell die-off},
  journal = {J.Appl.Microbiol.},
  year = {2007},
  volume = {102},
  pages = {1372--1381},
  number = {5},
  address = {Affiliation: Great Lakes Water Institute, University of Wisconsin-Milwaukee,
	Milwaukee, WI, United States; Affiliation: Marian College, Fond du
	Lac, WI, United States; Affiliation: Great Lakes Water Institute,
	University of Wisconsin-Milwaukee, (TRUNCATED)},
  annote = {Aims: The Escherichia coli burden at a Great Lakes urban beach was
	evaluated during the summer months to determine if sand served as
	a reservoir for E. coli, and if there was evidence of cell replication
	in situ. Field and laboratory studies investigated the effects of
	moisture, temperature and UV on E. coli densities in the sand. Methods
	and Results: Sand samples (n = 481) were collected across three distinct
	transects of the beach, the top, a middle streamline, and the berm,
	over 15 sample days. The highest levels were found in the middle
	streamline, which was affected by stormwater discharge from nearby
	outfalls and roosting gulls; daily geometric mean levels of these
	seven sites ranged from 6700 to 40 900 CFU per 100 g of sand. Escherichia
	coli levels were greatest in samples with moisture levels between
	15{\%} and 19{\%}, and were significantly higher than 0-4 and 20-24{\%}
	ranges (P < 0.05). Pre- and post-rain samples at the beach demonstrated
	an increase in E. coli levels nearly 100-fold within 30 min, suggesting
	sand washout as a major mechanism for loading of E. coli into the
	beach waters. Rep PCR analysis of 160 isolates obtained from eight
	sites demonstrated that 21{\%} of the isolates fell into one of the
	six clonal patterns, suggesting that bacteria may be able to replicate
	and possibly colonize beach sand. Sand field plots inoculated with
	E. coli cells containing pGFPuv that expresses GFP (green fluorescent
	protein) as a marker showed an initial two- to 100-fold increase
	at 24 h, depending on the temperature condition. The sand appeared
	to provide considerable protection from UV exposure as no significant
	difference was seen in cell densities within the first 2-4 cm of
	sand between exposed and unexposed plots (P < 0.05). Conclusions:
	Beach sand may act as a reservoir for E. coli. Replication of cells
	appears to be one possible contributing factor to the persistently
	high levels, as indicated by both field studies and laboratory studies,
	and warrants further investigation. Moisture content of sand may
	also be a determinant of cell persistence in the sand environment.
	Significance and Impact of the Study: Escherichia coli is used as
	an indicator organism for faecal pollution at most Great Lakes coastal
	beaches; therefore, a better understanding of how E. coli might survive,
	or possibly replicate, in the environment would improve interpretation
	of beach monitoring results. {\copyright}2007 The Authors.},
  annote1 = {Cited By (since 1996): 3},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/beversdorf_2007_potential_beach_sand_reservoir_ecoli_physical_influences_cell_die-off.pdf},
