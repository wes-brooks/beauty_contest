@ARTICLE{Abbott:1998ld,
  author = {Abbott,S. and Caughley,B. and Scott,G.},
  title = {Evaluation of Enterolert registered for the enumeration of enterococci
	in the marine environment},
  journal = {New Zealand Journal of Marine and Freshwater Research},
  year = {1998},
  volume = {32},
  pages = {505--513},
  number = {4},
  month = {Dec},
  address = {Wellington Polytechnic, Private Box 756, Wellington, New Zealand,
	{$[$}mailto:stan.abbott{\char64}wnp.ac.nz{$]$}},
  annote = {Current methods for the analyses of enterococcal densities include
	the membrane filtration (MF) technique and the multiple tube fermentation
	technique for the most probable number (MPN). Both techniques are
	labour intensive, tedious, and require a minimum of 48-72 h before
	results can be obtained. The Enterolert registered system, designed
	to detect enterococci in water in 24 h, uses 4-methylumbelliferyl-
	beta -D-glucoside as a defined substrate nutrient indicator. This
	compound, when hydrolysed by enterococcal- beta -glucosidase, releases
	4-methylumbelliferone which exhibits fluorescence under a UV sub(365)
	lamp. In this study 343 marine water samples from selected sites
	in the Wellington area of New Zealand were tested to evaluate the
	sensitivity and specificity of Enterolert in parallel with the MF
	method. Statistical analysis of parallel test results showed a strong
	linear correlation (r = 0.927) and no significant difference between
	the two methods by paired t-test analysis (P = 0.39). Based on the
	2.4{\%} false positive and 0.3{\%} false negative rates, Enterolert
	was found to have a sensitivity of 99.8{\%} and a specificity of
	97.0{\%}. Activity-costing analyses revealed that the variable cost
	per test was less for Enterolert (NZ{\$}18.33) than MF (NZ{\$}22.79).
	Significant time savings are achieved because Enterolert requires
	less time than MF for reagent preparation, sample set-up, incubation,
	and reading of tests. The results from this study suggest that more
	widespread use of this new technology in marine water quality monitoring
	is warranted, since rapid tests mean that monitoring agencies can
	respond to sudden increases in enterococci numbers more quickly and
	can therefore take immediate corrective action to ensure the safety
	of users of recreational waters.},
  annote1 = {TR: CS9908586 RX: 848516 (on Jul 11, 2008)},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/abbott_1998_enterolert_enterococci_marine.pdf},
  date-added = {2010-12-23 11:37:42 -0600},
  date-modified = {2010-12-23 11:37:42 -0600},
  isbn = {0028-8330},
  journal1 = {N.Z.J.Mar.Freshwat.Res.},
  keywords = {Article Subject Terms: Bacteria; Biological pollutants; Counting methods;
	Enumeration; Marine environment; Microbial contamination; Microbiological
	analysis; Pathogenic bacteria; Pollution detection; Pollution monitoring;
	Recreational waters; Article Taxonomic Terms: Enterococcus; Article
	Geographic Terms: PSE, New Zealand, North I., Wellington; Enterolert
	system; New Zealand},
  m1 = {Journal Article},
  ty = {JOUR},
  url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/abbott_1998_enterolert_enterococci_marine.pdf}
}

