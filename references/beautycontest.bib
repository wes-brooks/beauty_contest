  keywords = {Article Subject Terms: analytical techniques; bacteria; bathing; coastal
	waters; microbial contamination; pollution monitoring; water quality;
	Article Taxonomic Terms: Aeromonas; campylobacter; Article Geographic
	Terms: MED; Marine},
  m1 = {Book, Whole},
  ty = {BOOK}
}

