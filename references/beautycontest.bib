@book{Alonso:1993le,
	title = {Development and testing of sampling and analytical techniques for monitoring of marine pollutants (activity A). Final report on project on Control of Intestinal Pathogens (Campylobacter and Aeromonas) in marine coastal recreational areas},
	m = {Book, Whole},
	year = {1993},
	publisher = {UNEP},
	date = {2010-12-23 11:37:42 -0600},
	keywords = {Article Subject Terms: analytical techniques; bacteria; bathing; coastal waters; microbial contamination; pollution monitoring; water quality; Article Taxonomic Terms: Aeromonas; campylobacter; Article Geographic Terms: MED; Marine},
	ty = {BOOK},
	annote = {In the marine recreational waters analyzed, it has been demonstrated that m-T7 MF method can be used for the recovery of injured faecal coliforms. This technique has been shown to be as efficient as the most probable number (MPN) method. The mADA/0129 procedure has satisfied the criteria for a primary, selective and differential method for the enumeration of motile aeromonads from polluted seawaters. It appears that the exclusive use of the faecal bacterial indicators underestimate the risk of infection posed by opportunistic pathogens like Aeromonas spp. and it is considered advisable to monitor these bacteria in recreational waters where the risk of direct infection to humans is high. Epidemiological studies are however needed to determine the relationship between cases of Aeromonas-associated diarrhoea and the densities of theses organisms in marine recreational waters. The present study demonstrates also that marine recreational waters contain a variety of campylobacters and when environmental contamination does occur, the effect of sewage effluent discharged into marine waters is important. Knowledge of the survival characteristics of thermophilic campylobacters in seawater is of prime importance. Further studies are needed to identify possible indicator organisms which could be used to predict the likely presence of Campylobacter spp. in sewage polluted seawater. Data from this investigation provide information of seasonal occurrence of thermophilic campylobacters in marine waters and identification of the periods of greatest health hazard. However epidemiological studies are needed to elucidate the public health significance of campylobacters in marine recreational waters.},
	address = {Inst. Hydrol. Envir., Polytech. Univ., Valencia, Spain},
	author = {Alonso,JL and Amoros Munoz,I. and Salut Botella,M. and UNEP Mediterranean Action Plan, Athens (Greece)},
	booktitle = {UNEP, ATHENS (GREECE)}
}

@article{Alonso:1992jw,
	ty = {JOUR},
	url = {https://internal.mdc.usgs.gov/glrwq/RefLinks/alonso_1992_salmonella_detection_marine_standard_method.pdf},
	address = {Inst. Hidrol. y Medio Nat., Univ. Politec., 46071 Valencia, Spain},
	annote = {A study was undertaken to compare two enrichment broths and to test a new plate metdium, Rambach agar, for their suitability to detect Salmonella from marien recreational waters. The study shows that under conditions of low levels of fecal contamination, Salmonella spp was isolated more efficiently using NR10(10)/43 broth. While at higher levels of fecal contamination NR10/43 broth effectively limits the growth of accompanying flora. For the highest level of fecal contamination Rambach agar was less selective than Hektoen enteric agar but Rambach agar was shown to be better than Hektoen agar for the recovery of Salmonella strains from low contaminated waters. On Rambach agar the distinct color of salmonellas, seen in red, enhanced correct identification. Biochemical and serological confirmation of Salmonella isolates was performed using the API Z System and Oxoid Salmonella Rapid Latext test. The combination of enrichment broths (24 h at 42 degree C), isolation (24 h at 27 degree C) and identification systems described above (2 h or 1 min) allowed the detection of Salmonella from marine waters, with low or high levels of fecal contamination, in 48 h.},
