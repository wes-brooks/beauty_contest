@ARTICLE{Davies:2003zt,
  author = {Davies,C. M. and Yousefi,Z. and Bavor,H. J.},
  title = {Occurrence of coliphages in urban stormwater and their fate in stormwater
	management systems},
  journal = {Letters in Applied Microbiology},
  year = {2003},
  volume = {37},
  pages = {299--303},
  annote = {Aims: To investigate the occurrence of coliphages in, and their removal
	from, urban stormwater. Methods and Results: Inflow and outflow concentrations
	of somatic and f-specific RNA coliphages to two stormwater treatment
	systems were determined on 21 occasions over a period of 5 months.
	Somatic coliphages were detected in 19 (90%) of the constructed wetland
	inlet samples, 13 (62%) of the pond inlet samples, and less frequently
	at the outlets of the two systems. F-specific RNA coliphages were
	detected at the inlets but only occasionally at the pond outlet.
	Somatic coliphages were found to attach preferentially to particles
	<5 lm in size and persisted in the sediments of the two systems.
	Conclusions: Treatment systems providing conditions that are conducive
	to the settlement of fine particles may effectively remove sediment-bound
	coliphages and, therefore, possibly enteric viruses from stormwater.
	Significance and Impact of the Study: The results will aid the design
	of systems for effective removal of viral contaminants from urban
	stormwater.},
  bdsk-url-1 = {https://internal.mdc.usgs.gov/glrwq/RefLinks/davies_2003_occurrence_coliphages_urban_stormwater_fate_stormwater_management_systems.pdf},
