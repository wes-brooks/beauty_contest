  date-modified = {2010-12-23 11:37:42 -0600},
  isbn = {0043-1354},
  keywords = {Beach water quality; Cladophora--Salmonella association; Enteric bacteria;
	Environmental survival; Lake Michigan; Public health},
  m1 = {Journal Article},
  ty = {JOUR},
