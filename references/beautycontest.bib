  keywords = {BUOYANCY; DIFFUSION; FAR-FIELD EFFECTS; JETS; MIXING; NEAR FIELD EFFECTS;
	PLUMES; SEWAGE DISPOSAL},
  m1 = {Journal Article},
  sp = {1975},
  ty = {JOUR},
  url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0016462341&partnerID=40}
}

