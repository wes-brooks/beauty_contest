@ARTICLE{OKeefe:1989zp,
  author = {O'Keefe,B. O. and Green,J.},
  title = {Coliphages as indicators of faecal pollution at three recreational
	beaches on the Firth of Forth},
  journal = {WATER RES.},
  year = {1989},
  volume = {23},
  pages = {1027--1030},
  number = {8},
  address = {Affiliation: Department of Microbiology, Forth River Purification
	Board, Edinburgh EH13 0PH, United Kingdom; Correspondence Address:
	Keefe, Brian O.; Forth River Purification BoardUnited Kingdom},
  annote = {This study was designed to compare the levels of bacteriophages specific
	to Escherichia coli to the levels of bacterial indicators in the
	water at three recreational beaches on the Firth of Forth. Beaches
	were chosen on the basis that they were known to suffer widely different
	degrees of faecal contamination. A direct plaque assay using 100
	ml water samples was used to enumerate coliphages, and total coliforms
	and faecal coliforms were enumerated by membrane filtration. There
	was a high degree of correlation between the levels of coliphages
	and coliforms between the three sites studied. Within sites, levels
	of coliphages and coliforms showed a high degree of correlation on
	the two highly-polluted beaches, whereas correlation was low on the
	less-polluted beach. At the less-polluted beach coliphages were detected
	in the absence of faecal coliforms on two occasions, whereas faecal
	coliforms were detected in the absence of coliphages on one occasion.
	The results suggest that coliphages are as adequate indicators of
	faecal pollution at the sites studied as are coliform bacteria. This
