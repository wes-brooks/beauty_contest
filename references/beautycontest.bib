@article{Kaye:1992rg,
	booktitle = {Proceedings of the IAWPRC Specialized Conference},
	address = {Affiliation: Centre for Wastewater Treatment, The University of New South Wales, PO Box 1, Kensington, NSW 2033, Australia; Correspondence Address: Kaye, R.B.; Centre for Wastewater Treatment; The University of New South Wales; PO Box 1 Kensingt(TRUNCATED)},
	isbn = {02731223},
	keywords = {Coliform bacteria; Effluents; Fluorescence; Marine pollution; Monitoring; Sewage bacteriology; Wastewater; Sydney Harbour; Outfalls; sea water; australia; coliform bacterium; conference paper; density; effluent; excitation; feces microflora; nonhuman; performance; public health; sewage disposal; streptococcus; water sampling; Bacteria; Bathing Water; Indicators; Sea Outfalls; Sydney, Australia; Water Quality},
	pages = {41--48},
	title = {Compliance testing for ocean outfall performance utilising natural fluorescence of effluent in seawater mixtures},
	url = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0026614719&partnerID=40},
	number = {9},
	journal = {WATER SCI.TECHNOL.},
	m = {Journal Article},
	ty = {JOUR},
	month = {20 November 1991 through 22 November 1991},
	bdsk = {http://www.scopus.com/scopus/inward/record.url?eid=2-s2.0-0026614719&partnerID=40},
	year = {1992},
	annote = {Water samples were collected from public beaches at Sydney, Australia. The beaches were situated at various distances from the three major shoreline ocean outfalls discharging primary effluent. The samples were tested for faecal coliform and faecal streptococci indicator bacteria. The same samples were also analysed for natural fluorescence. Excitation wavelength used was 280 nm and fluorescence emission from seawater effluent mixtures was observed at 445 nm. Indicator bacteria densities correlated well with fluorescence intensities for beach monitoring stations north of Sydney Harbour. Best correlations were observed for the stations situated within 5 kilometres from the outfall. Indicator bacteria densities were not as well correlated with fluorescence intensities for the stations south of Sydney Harbour. However, similar emission spectra were observed for all samples collected from stations both north and south of Sydney Harbour. Nevertheless, indicator bacteria densities for southern stations were observed to be better correlated with fluorescence intensities on particular days. The stations north of Sydney Harbour are believed to be impacted only by the one outfall at North Head. However the system south of Sydney Harbour is more complex. The southern stations are known to be impacted by any, or all of the three major outfalls at Bondi, Malabar, and North Head. Fluorescence can be used to determine effluent concentration in ocean waters where the origin of the effluent is known. There should also be opportunities for the development of fluorescence as an alternative indicator for 'real time' public health monitoring of recreational waters.},
	volume = {25},
	author = {Kaye,R. B. and Haddad,P. R.},
	date = {2010-12-23 11:37:42 -0600}
}

