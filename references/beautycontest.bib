	author = {Gray,J. S.},
	year = {1976},
	date = {2010-12-23 11:37:43 -0600},
	journal = {ESTUARINE COASTAL MAR.SCI.},
	pages = {653--676},
	title = {The fauna of the polluted river Tees estuary},
